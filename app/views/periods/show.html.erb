<div class="container">
  <div class="row">
    <div class="col-sm-12" style="text-align:center;">
      <h1>Period</h1>
    </div>
  </div>
  <div class="row" >
    <div class="col-sm-2">
      From date: 
    </div>
    <div class="col-sm-10">
      <%= @period.from_date %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-2">
      To date: 
    </div>
    <div class="col-sm-10">
      <%= @period.to_date %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-2">
      Status: 
    </div>
    <div class="col-sm-10">
      <%= @period.status %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-2">
      Created at: 
    </div>
    <div class="col-sm-10">
      <%= @period.created_at %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-2">
      Updated at: 
    </div>
    <div class="col-sm-10">
      <%= @period.updated_at %>
    </div>
  </div>
</div>
