<div class="title-table-form-preview-result">CDS Assessment Point</div>
<div style="overflow-x:auto;">
  <table class="table table-striped table-primary table-responsive-sm table-preview-result">
    <thead>
      <tr>
        <td>Competency</td>
        <td class="form-pr-level">Level</td>
        <td>1A</td>
        <td>1B</td>
        <td>1C</td>
        <td>1D</td>
        <td>1E</td>
        <td>1F</td>
        <td>1G</td>
        <td>2A</td>
        <td>2B</td>
        <td>2C</td>
        <td>2D</td>
        <td>2E</td>
        <td>2F</td>
        <td>2G</td>
        <td>3A</td>
        <td>3B</td>
        <td>3C</td>
        <td>3D</td>
        <td>3E</td>
        <td>3F</td>
        <td>3G</td>
        <td>4A</td>
        <td>4B</td>
        <td>4C</td>
        <td>4D</td>
        <td>4E</td>
        <td>4F</td>
        <td>4G</td>
        <td>5A</td>
        <td>5B</td>
        <td>5C</td>
        <td>5D</td>
        <td>5E</td>
        <td>5F</td>
        <td>5G</td>
      </tr>
    </thead>
    <tbody>
      <% @competencies.each_with_index do |c, i| %>
        <tr>
          <td class="form-competency-name"><%= c %></td>
          <td class="form-pr-level">++1</td>
          <% @slots.each do |s| %>
            <td class="<%= @result[c][s][:type] + " " + @result[c][s][:class] unless @result[c][s].nil? %>"><%= @result[c][s].nil? ? "0" : @result[c][s][:value] %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>


