<div class="modal " tabindex="-1" id="modalAddAssessment" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header head-myheader">
        <h5 class="modal-title">Create a New CDS</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container">
          <div class="row">
            <div class="col-sm-4">
              Period
            </div>
          </div>
          <div class="row" style="margin-top: 25px;margin-bottom: 25px">
            <div class="col-sm-4">
              From:
            </div>
            <div class="col-sm-6">
              <input type="text" id="from_date" name="from_date" class="form-control">
            </div>
          </div>
          <div class="row" style="margin-top: 25px;margin-bottom: 25px">
            <div class="col-sm-4">
              To:
            </div>
            <div class="col-sm-6">
              <input type="text" id="to_date" name="to_date" class="form-control">
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="modal-footer">
      <button type="button" id="add_assessment_list_button" class="btn btn-primary" data-dismiss="modal">Create</button>
      <button type="button" class="btn btn-light border-dark" data-dismiss="#modalAdd">Cancel</button>
    </div>
  </div>
</div>
</div>
