      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(363, 'Identify and solve complex or sensitive problems at the strategic executive level.', '<p><em><span style="text-decoration: underline;"><strong>Ví dụ:</strong></span></em></p><p><em>+ Phát biểu bài toán 7 năm của công ty và giải quyết nó</em></p><p><em>+ Phát biểu bài toán technical lead của công ty và giải quyết nó</em></p><p><em>+ Phát biểu bài toán quản lý chi phí / năng suất của công ty và giải quyết nó</em></p>', 4, 28, 19);
