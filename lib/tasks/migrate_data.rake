      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(131, 'Lead simple internal meetings.  Peers and / or next level management are likely to attend these meetings', '', 2, 20, 131);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(132, 'Capable of client interaction with supervision or guidance from management or team leadership', '', 2, 20, 132);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(133, 'Exhibit developed good written and verbal communications. This is evident in complete, accurate and timely status reports and project deliverables as well as clear and concise direct communication.', '<p><strong>Good:</strong> Có khả năng tốt về written &amp; verbal communications, ít khi phải hỏi ai về written &amp; verbal communications. Có thể chịu trách nhiệm chính về written &amp; verbal communications cho 1 hay nhiều dự án [giới hạn ở vai trò của mình].</p>', 2, 20, 133);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(134, 'Demonstrated ability to transfer knowledge to junior associates and thus serve as a mentor.', 'Cho sinh viên thực tập hay cho nhân viên mới? Bao nhiêu người? Level mentoring cỡ nào?', 2, 20, 134);
