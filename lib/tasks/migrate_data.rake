      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(367, 'Lead simple internal meetings. Peers and / or next level management are likely to attend these meetings', '', 2, 29, 23);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(368, 'Capable of client interaction with supervision or guidance from management or team leadership', '', 2, 29, 24);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(369, 'Exhibit developed good written and verbal communications. This is evident in complete, accurate and timely status reports and project deliverables as well as clear and concise direct communication.', '<p><strong>Good:</strong> Có khả năng tốt về written &amp; verbal communications, ít khi phải hỏi ai về written &amp; verbal communications. Có thể chịu trách nhiệm chính về written &amp; verbal communications cho 1 hay nhiều dự án [giới hạn ở vai trò của mình].</p>', 2, 29, 25);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(370, 'Demonstrated ability to transfer knowledge to junior associates and thus serve as a mentor.', 'Cho sinh viên thực tập hay cho nhân viên mới? Bao nhiêu người? Level mentoring cỡ nào?', 2, 29, 26);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(371, 'Effectively deliver unpopular and/or difficult messages with an understanding of the receiver’s viewpoint.', '<p>Một key member [Senior Software Engineer hoặc tương ứng trở lên] muốn rời khỏi công ty, thậm chí đã gửi đơn xin nghỉ việc. Thuyết phục người này ở lại bằng cách nào?</p><p>Khách hàng không có ngân sách thuê ngoài dịch vụ training về quản lý dự án trong khi BESTARION lại .... làm sao để thuyết phục khách hàng?</p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty. Ví dụ cụ thể: Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</p><p> </p>', 3, 29, 27);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(372, 'Demonstrate strong written and verbal communication skills. (Example: formal documents, client/peer or conference presentations).', '<p><strong>Strong:</strong> written &amp; verbal communications là sở trường, lợi thế, sở thích. Được khách hàng, đồng nghiệp, cấp trên khen ngợi, bái phục. Được mọi người tham khảo ý kiến khi cần communicate cho tốt hơn</p><p><em><span style="text-decoration: underline;"><strong>Phản ví dụ:</strong> </span>Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty. <em><span style="text-decoration: underline;"><strong>Ví dụ cụ thể:</strong></span> Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</em></p>', 3, 29, 28);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(373, 'Lead or facilitate complex meetings for either internal or external clients. The next level of management likely attends these meetings.', '<p><em><span style="text-decoration: underline;"><strong>Example:</strong></span> formal reviews, workshops, requirements sessions</em></p>', 3, 29, 29);
