      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(55, 'Successfully handle and organize strategy task', '<p><span style="text-decoration: underline;"><strong>Một số ví dụ:</strong></span></p><p><em>Handle &amp; Organize CMMI Level 3 project</em></p><p><em>Handle &amp; Organize Portfolio Management</em></p><p><em>Handle &amp; Organize: Quản lý năng suất tại công ty</em></p><p><em>Handle &amp; Organize: Quản lý nhân sự tại công ty – bài toán giữ người, bài toán lương thưởng / phúc lợi</em></p>', 4, 4, 55);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(56, 'Successfully manage and organize multiple tasks at the company level', 'Successfully manage and organize multiple tasks at the company level', 4, 4, 56);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(57, 'Accountable for developing his / her own management skill set to meet the company expectation. ', '<p>PMP, MBA là điều kiện cần</p><p>Cần có bằng chứng về việc áp dụng management skill set thành công</p>', 4, 4, 57);
