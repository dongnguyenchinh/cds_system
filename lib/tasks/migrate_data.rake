        (3342, 'Hoàn thành tốt công việc trong mức độ năng suất tốt, sai sót thấp', 3, true, '', 3342, now(), now(), false),
        (3343, 'Có đóng góp giúp đỡ team trong việc giải đáp các thắc mắc về vấn đề kỹ thuật, giao tiếp. hỗ trợ người mới trong các vấn đề thường gặp. có khả năng hỗ trợ từ yêu cầu của các team khác như bên datatech, bên cian, mdonboard.', 3, true, '', 3343, now(), now(), false),
        (3344, 'Số lượng task support hoàn thành đúng hạn, giao tiếp ngay khi cần constraint từ bên khác, và biết cách push hợp lý để task đc resolved đúng hẹn. đối với task build có khả năng giao tiếp để nắm rõ hơn về requirement, gợi ý thêm cho requirement', 3, true, '', 3344, now(), now(), false),
        (3345, 'Tuân thủ quy tắc trong phòng làm việc của mars, các chuẩn mực của larion nói chung', 3, true, '', 3345, now(), now(), false),
        (3346, 'Có sử dụng được tài liệu từ larion portal, tài liệu từ bên client dành cho các guidelines, instruction đọc hiểu được tài liệu nhận được từ vendor', 3, true, '', 3346, now(), now(), false),
        (3347, 'Tuự động raise lên khi cần sự hộ trợ, raise cho đúng người đúng hoàn cảnh. có khả năng call for meeting khi có khúc mắc những vấn đề lớn ảnh hưởng team.\nvd: call for meeting có sự tham gia của team cian và hà nội để giải thích một vấn đề mà cả team còn khúc mắc (data point)', 3, true, '', 3347, now(), now(), false),
        (3348, 'Nhận task và hoàn thành task critical đúng hẹn.\nvd:\nnhận được task tìm một thư viện phù hợp để xử lý packet trading của một vendor\n-tìm hiểu tất cả các thư viện hiện có:(thử nhiều ngôn ngữ khác nhau trên môi trường linux) và đưa kết quả\n- tiến hành truy suất dữ liệu trong packet ra và sắp xếp thành các subset nhỏ phù hợp để báo cáo\n- cài đặt trên môi trường local để xử lý packet.\n- commit code for reviewing → thực hiện testing cho dữ liệu mẫu\n- observing và kiểm tra chất lượng data\n\ntổng thời gian:3 tuần', 3, true, '', 3348, now(), now(), false),
        (3349, 'Training cho người mới của team atrax, hướng dẫn các vấn đề thông thường.\nđại diện client tham gia một meeting của vendor để thu thập thông tin.\n\nvd: đại diện client tham gia meeting với vendor để thu thập thông tin api cho việc build dataset. thông tin thu thập đầy đủ và không phải recontact lại', 3, true, '', 3349, now(), now(), false),
        (3350, 'Đã pass ở kỳ trước', 2, true, '', 3350, now(), now(), false),
        (3351, 'Vd: làm các task một cách độc lập và hiệu quả, cần cải thiện kỹ năng cập nhật status để người khác dễ track.', 2, true, '', 3351, now(), now(), false),
        (3352, 'Xác định các vấn đề phát sinh, các vấn đề tiềm tàng trong tương lai gây tổn hại chi phí. giải quyết và cải thiện các vấn đề tồn đọng', 3, true, '', 3352, now(), now(), false),
        (3353, 'Có khả năng làm việc độc lập, đưa ra giải pháp tốt nhất cho nhiều vấn đề. kiến thức business của client đủ tốt để đưa ra hướng giải quyết khi người khác hỏi', 3, true, '', 3353, now(), now(), false),
        (3354, 'Có khả năng đề xuất được hướng giải quyết cho các vấn đề phức tạp, đưa ra hướng giải quyết cho người khác cân nhắc', 3, true, '', 3354, now(), now(), false),
        (3355, 'Host được weekly report meeting, host được meeting dành cho việc chia sẻ thông tin với co-worker nước ngoài, host được meeting dành cho vấn đề thắc mắc của team có sự tham gia  của của leader.\nvd: lead meeting về các vấn đề thắc mắc và đề xuất cho team', 3, true, '', 3355, now(), now(), false),
        (3356, 'Đã pass ở kỳ trước', 3, true, '', 3356, now(), now(), false),
        (3357, 'Hằng ngày có report daily work, hằng tuần có report trong weekly meeting, khi có request biết cách tạo ticket, biết verify thông tin input với bên cung cấp. có khả năng làm việc và hỗ trợ remote với các team khác', 3, true, '', 3357, now(), now(), false),
        (3358, 'Hiện tại team có 3 meeting hằng tuần, sử dụng tốt tiếng anh để hỏi và trả lời, có khả năng tiếp nhận thông tin từ các team ở ấn độ, hà nội', 3, true, '', 3358, now(), now(), false),
        (3359, 'Có khả năng lead một meeting có sẵn agenda hoặc không có agenda với sự tham dự của cấp bậc quản lý.\nvd: weekly meeting host sẽ xoay tua các thành viên trong team mỗi tuần.\ncó một vấn đề cả team thắc mắc và cảm thấy không rõ ràng. biết call for meeting và tự set up agenda, mời những thành viên liên quan và người quản lý. biết đưa ra vấn đề và đặt câu hỏi, đề suất giải pháp và đưa ra kết quả cuối cùng', 3, true, '', 3359, now(), now(), false),
        (3360, 'Có khả năng tương tác với client hiệu quả mà không cần supervision và guidance, có khả năng tương tác với vendor onbehalf of client\nvd: đặc thù công việc của dự án yêu cầu tương tác với nhiều team trên nhiều quốc gia', 3, true, '', 3360, now(), now(), false),
        (3361, 'Khả năng truyền đạt thông tin tốt, ít gây hiểu lầm, có dẫn chứng\nreport một vấn đề khi mở ticket và close đầy đủ thông tin, và có ý nghĩa.\nkhả năng truyền đạt tốt cả tiếng việt lẫn tiếng anh.\nvd: công việc đa phần là cần nhiều constraint từ nhiều bên, việc giao tiếp bằng lời nói (call trực tiếp) và thông qua văn bản (ticket comment và direct chat) là thường xuyên', 3, true, '', 3361, now(), now(), false),
        (3362, 'Khả năng truyền đạt thông tin cho team, cho người mới\nvd: các vấn đề tự tìm hiểu ra, liên quan tới team khác(datatech, mdonboard,...) có truyền đạt cho team để nắm rõ hơn. khi đc assign việc mới thì có chia sẽ để team có kiến thức về quy trình việc mình đang làm.\ntraining cho người mới nhiệt tình giúp không bị ngán ngẩm công việc và nhanh chóng keep up với team.', 3, true, '', 3362, now(), now(), false),
        (3363, 'Truyền đạt thông tin phức tạp bằng tiếng việt tốt, truyền đạt thông tin phức tạp bằng tiếng anh vẫn chưa thỏa mản được nhiều người', 2, true, '', 3363, now(), now(), false),
        (3364, 'Tham dự và đóng góp tốt vai trò của mình trong meeting lớn của team, hỗ trợ đóng mở vấn đề liên quan để giúp cuộc họp trở nên suôn sẻ hơn:\nvd: họp meeting cả team hàng tuần, report lần lượt đến từ từng người có vai trò hôm đó, đưa ra một báo cáo đầy đủ và ngắn gọn về thông tin dataset của cả team tuần vừa qua. giải đáp thắc mắc khi có các câu hỏi từ bên khác, đóng vấn đề ngay khi chuẩn bị đi quá xa và kết thúc phần báo cáo.', 3, true, '', 3364, now(), now(), false),
        (3365, 'Thích nghi với hệ thống của kh nhanh mặc dù chưa từng làm python.\ncó khả năng làm task với yêu cầu khác biệt với ngôn ngữ thường dùng(tìm hiểu nhiều ngôn ngữ để build 1 module hỗ trợ cho dataset đặc biệt, cuối cùng sử dụng java)\ntrình độ av cải thiện đúng thời điểm', 3, true, '', 3365, now(), now(), false),
        (3366, 'Báo cáo được các điểm chính trong meeting dataset, thống kê các điểm chính để báo cáo trong meeting cả team. team có viết report weekly để báo cáo về việc đang làm', 3, true, '', 3366, now(), now(), false),
        (3367, 'Các task hot fix thì có khả năng xác định và ước tính được khoảng thời gian là ngắn và độ ưu tiên cao, fix ngay trong ngày. các task build mới thì xác định độ phức tạp để có thể ước tính được là hoàn thành trong 1 tuần hay 2.\nvd: các task build mới ước lượng đc thời gian hoàn thành trong 2 tuần thì sẽ hoàn thành stage building trong 2 tuần. khi xuất hiện thêm nhiều đề xuất cho recommend làm tăng độ phức tạp cải dataset hơn thì sẽ tính toán lại thời gian.', 3, true, '', 3367, now(), now(), false),
        (3368, 'Các task nhỏ được fix xen kẽ với các task lớn phải chờ verify hoặc hỗ trợ từ nhiều phía, trong khi chờ code chạy tranh thủ làm task khác\ncác task có độ ưu tiên cao nhất thì ưu tiên làm trước\ncác task với độ phức tạp thấp thì sẽ được xử lý song song', 3, true, '', 3368, now(), now(), false),
        (3369, 'Task được giải quyết nhanh nhất có thể ngay sau khi take over, vấn đề sẽ được báo cáo ngay trong meeting gần nhất. với vấn đề liên quan đến compliance hoặc ảnh hưởng lớn sẽ được report ngay khi detect đc\nvd: việc scrape một website bị yêu cầu chấp nhận điều khoản trước khi đc access data thì sẽ đc report sớm nhất', 3, true, '', 3369, now(), now(), false),
        (3370, 'Object của kh đề ra được ưu tiên, client đang hài lòng với cách làm việc của team', 3, true, '', 3370, now(), now(), false),
        (3371, 'Các task nhỏ được fix xen kẽ với các task lớn phải chờ verify hoặc hỗ trợ từ nhiều phía, trong khi chờ code chạy tranh thủ làm task khác\ncác task có độ ưu tiên cao nhất thì ưu tiên làm trước\ncác task với độ phức tạp thấp thì sẽ được xử lý song song\nluôn hoàn thành task task support trong vòng 1,2 ngày', 3, true, '', 3371, now(), now(), false),
        (3372, 'Nhận task và hoàn thành task critical đúng hẹn.\nvd:\nnhận được task tìm một thư viện phù hợp để xử lý packet trading của một vendor\n-tìm hiểu tất cả các thư viện hiện có:(thử nhiều ngôn ngữ khác nhau trên môi trường linux) và đưa kết quả\n- tiến hành truy suất dữ liệu trong packet ra và sắp xếp thành các subset nhỏ phù hợp để báo cáo\n- cài đặt trên môi trường local để xử lý packet.\n- commit code for reviewing → thực hiện testing cho dữ liệu mẫu\n- observing và kiểm tra chất lượng data\n\ntổng thời gian:3 tuần', 3, true, '', 3372, now(), now(), false),
