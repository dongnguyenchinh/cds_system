      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(129, 'Capable of internal interaction. This is evident in all core communications.', '<p><span style="text-decoration: underline;"><em><strong>Example:</strong></em></span><br /><em>Writing: Weekly report, email, Bug tracker, update document, etc.</em><br /><em>Reading and understanding: Project documents, Company policy, Guideline(Bug tracker, SVN, HRM), etc. </em><br /><em>Communicating with team member: discuss and raise issues as soon as they are known</em></p>', 1, 20, 129);
