      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(21, 'Capable of internal interaction. This is evident in all core communications.', '<p><em><span style="text-decoration: underline;"><strong>Example:</strong> </span></em><br /><em>Writing: Weekly report, email, Bug tracker, update document, etc.</em><br /><em>Reading and understanding: Project documents, Company policy, Guideline (Bug tracker, SVN, HRM), etc. </em><br /><em>Communicating with team member: discuss and raise issues as soon as they are known</em></p>', 1, 3, 21);
