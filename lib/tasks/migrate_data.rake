      insert into slots(id, `desc`, evidence, level, competency_id, slot_id, created_at, updated_at) values
        (449, "Consistently perform well under supervision", "<p>Nếu được giao việc và được hướng dẫn cách làm thì hoàn tất tốt công việc được giao trong mức độ năng suất hợp lý<br />Nếu có sai sót trong quá trình làm việc và đã được nhắc nhở về những lỗi cụ thể thì không lặp lại những lỗi này.</p>", 1, 35, 449, now(), now()),
        (450, 'Recognized as a contributing member of the team.', '<p>Là một thành viên đóng góp tích cực cho nhóm / dự án. Những người khác không thấy nhân viên này là điểm yếu, là gánh nặng cho team trong productivity</p><p>Phản ví dụ: Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</p>', 1, 35, 450, now(), now()),
        (451, 'Complete assigned tasks within scheduled completion dates. Communicate potential issues as soon as you are known.', '<p>Hoàn tất công việc được giao một cách đúng hạn, với chất lượng tốt. Trao đổi, thông báo ngay về những vấn đề tiềm năng khi phát hiện ra chúng</p>', 1, 35, 451, now(), now()),
        (452, 'Demonstrate a high level understanding of and compile with LARION standards, processes and procedures.', '<p>Hiểu rõ và tuân thủ tốt các quy trình, quy định, thủ tục, chuẩn do LARION đang áp dụng</p>', 1, 35, 452, now(), now()),
        (453, 'Capable of locating and effectively using detailed information from the LARION Portal, Mantis, SVN, HRM,…', '', 1, 35, 453, now(), now()),
        (454, 'Recognized as a proactive member of the team.', "<p>Là một thành viên đóng góp tích cực cho nhóm / dự án. Nằm trong số những người đóng góp hàng đầu cho dự án.</p><p><strong><span style='text-decoration: underline;'><em>Phản ví dụ</em></span>:</strong> Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</p>", 2, 35, 454, now(), now()),
        (455, 'Complete critical tasks on time', '<p>Critical tasks: Những công việc quan trọng, chủ chốt, cốt lõi trong dự án, trong bộ phận mà nhân viên tham gia, ở góc độ vai trò tương ứng.</p><p>Ví dụ: <br />Nhân viên làm Business Analyst trong một dự án thì critical tasks là những công việc chủ chốt, cốt lõi liên quan đến phân tích yêu cầu khách hàng</p><p>Nhân viên làm Developer trong một dự án thì critical tasks là những công việc liên quan đến thiết kế / tài liệu hóa / lập trình / sửa bugs của những thứ quan trọng, chủ chốt, cốt lõi.</p><p>Nhân viên làm Tester trong một dự án thì thì critical tasks là những công việc liên quan đến việc phát triển test plan / test cases / tiến hành test execution của những thứ quan trọng, chủ chốt, cốt lõi trong dự án.</p>', 2, 35, 455, now(), now()),
        (456, 'Able to represent the skills and capabilities of your department/ practice unit/business unit. ', '<p>Represent: Đại diện.</p><p>Có nhiều mức độ đại diện – ví dụ</p><p>Đại diện cho dự án để training cho một nhân viên mới, training cho sinh viên thực tập về kiến thức, kỹ năng ở vai trò của mình;</p><p>Đại diện cho dự án để làm việc với dự án khác; đại diện cho dự án làm việc với khách hàng;</p><p>Có khả năng chịu trách nhiệm chính trong dự án về chuyên môn mà mình đang đảm nhận;</p><p>Đại diện cho bộ phận chuyên môn của mình ở phạm vi công ty để làm việc với bất cứ đối tượng nào khi cần</p><p>Nhân viên sẽ được đánh giá pass slot này nếu có bằng chứng thuyết phục liên quan đến khả năng chịu trách nhiệm chính về chuyên môn trong dự án hiện tại và có khả năng đại diện cho bộ phận chuyên môn của mình ở phạm vi công ty để làm việc với bất cứ đối tượng nào khi cần</p>', 2, 35, 456, now(), now()),
        (457, 'Recognized internally as a solid knowledge resource.', '<p>Cứ có vấn đề gì về mảng kiến thức mà cần trợ giúp / câu trả lời là nghĩ ngay đến nhân viên này</p>', 3, 35, 457, now(), now()),
        (458, 'Successfully complete tasks and assignments independently and supervise the work of others as requested.', '<p>Nhân viên sẽ được đánh giá pass slot này nếu có bằng chứng thuyết phục:<br />- Khi được giao việc thì hoàn tất được công việc một cách độc lập, không cần có sự chỉ dẫn về cách làm, và<br />- Có thể hướng dẫn, hỗ trợ, giám sát người khác [vài người] hoàn tất công việc</p>', 3, 35, 458, now(), now()),
        (459, 'Regularly requested by Company or Business Unit leadership and clients for advice and guidance.', '<p>Có khả năng tư vấn, định hướng, dẫn đường cho công ty hoặc khách hàng về: <br />- Chuyên môn kỹ thuật ở vai trò mình nắm giữ, hoặc <br />- Trong những vấn đề liên quan đến hoạt động chung của công ty</p>', 3, 35, 459, now(), now()),
        (460, 'Identify problems, think through potential solutions then communicate and/or escalate appropriately.', "<p><span style='text-decoration: underline;'><em><strong>Phản ví dụ</strong></em></span>: Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</p>", 1, 36, 460, now(), now()),
        (461, 'Identify and solve simple problems independently (for example: recognizing changes in scope and communicating them).', '<p>Không cần hỏi người khác v������������ giải pháp cho vấn đề [đơn giản] đang gặp phải</p>', 2, 36, 461, now(), now()),
        (462, 'Provide assistance in solving complex problems (complex – technically difficult, opposing viewpoints, risky, and / or sensitive).', '<p>Đề xuất, hỗ trợ được trong việc giải quyết các vấn đề phức tạp</p>', 2, 36, 462, now(), now()),
        (463, 'Lead simple meetings for internal or external clients. Peer and / or next level management are likely to attend these meetings. ', '<p>Bằng chứng thuyết phục là có dính dáng đến khách hàng bên ngoài</p>', 2, 36, 463, now(), now()),
        (464, 'Possess and use good diagnosis / troubleshooting skills.', '', 2, 36, 464, now(), now()),
        (465, 'Identifies and solves complex or sensitive problems (for example: associate performance, project scope issues, and priority changes or resolving multiple conflicting agendas). ', '<p>- Không cần hỏi người khác về giải pháp cho vấn đề phức tạp đang gặp phải.<br />- Thế nào là vấn đề phức tạp hoặc nhạy cảm trong phát triển phần mềm?<br />- Yêu cầu của khách hàng phức tạp, logic xử lý phức tạp, giải thuật phức tạp và có đòi hỏi cao về performance, security; đòi hỏi hiểu biết cao để ứng dụng các frameworks sẵn có thay vì phải hiện thực từ đầu; phải xử lý các vấn đề phức tạp đồng thời</p>', 3, 36, 465, now(), now()),
        (466, 'Breaks down major issues and problems into workable pieces; sees whole situation behind a problem or issue;understands the causes, effects, implications; sets priorities and takes right action for resolution.', '<p>- Có khả năng chia nhỏ vấn đề lớn, phức tạp thành những phần nhỏ hơn để xử lý;</p><p>- Nắm được bức tranh tổng thể của một vấn đề phức tạp; hiểu rõ nguyên nhân, hậu quả, những vấn đề có liên quan; thiết lập được độ ưu tiên, thứ tự và tiến hành các công việc phù hợp để giải quyết bài toán</p>', 3, 36, 466, now(), now()),
        (467, 'Identify and solve complex or sensitive problems at the strategic executive level.', "<p><strong><span style='text-decoration: underline;'>Ví dụ:</span></strong></p><p>Phát biểu bài toán 7 năm của công ty và giải quyết nó</p><p>Phát biểu bài toán technical lead của công ty và giải quyết nó</p><p>Phát biểu bài toán quản lý chi phí / năng suất của công ty và giải quyết nó</p>", 4, 36, 465, now(), now()),
        (468, 'Anticipate project and / or assignment risks and provide potential solutions.', '', 4, 36, 466, now(), now()),
        (469, 'Capable of internal interaction. This is evident in all core communications.', "<p><span style='text-decoration: underline;'><em><strong>Example:</strong> </em></span><br />Writing: Weekly report, email, Bug tracker, update document, etc.<br />Reading and understanding: Project documents, Company policy, Guideline (Bug tracker, SVN, HRM), etc. <br />Communicating with team member: discuss and raise issues as soon as they are known</p>", 1, 37, 467, now(), now()),
        (470, 'Effectively attend simple meetings', '', 1, 37, 468, now(), now()),
        (471, 'Lead simple internal meetings. Peers and / or next level management are likely to attend these meetings', '', 2, 37, 469, now(), now()),
        (472, 'Capable of client interaction with supervision or guidance from management or team leadership', '', 2, 37, 470, now(), now()),
        (473, 'Exhibit developed good written and verbal communications. This is evident in complete, accurate and timely status reports and project deliverables as well as clear and concise direct communication.', '<p><strong>Good</strong>: Có khả năng tốt về written &amp; verbal communications, ít khi phải hỏi ai về written &amp; verbal communications. Có thể chịu trách nhiệm chính về written &amp; verbal communications cho 1 hay nhiều dự án [giới hạn ở vai trò của mình].</p>', 2, 37, 471, now(), now()),
        (474, 'Demonstrated ability to transfer knowledge to junior associates and thus serve as a mentor.', '<p>Cho sinh viên thực tập hay cho nhân viên mới? Bao nhiêu người? Level mentoring cỡ nào?</p>', 2, 37, 472, now(), now()),
        (475, 'Effectively deliver unpopular and / or difficult messages with an understanding of the receiver’s viewpoint.', '<p>Một key member [Senior Software Engineer hoặc tương ứng trở lên] muốn rời khỏi công ty, thậm chí đã gửi đơn xin nghỉ việc. Thuyết phục người này ở lại bằng cách nào?</p><p>Khách hàng không có ngân sách thuê ngoài dịch vụ training về quản lý dự án trong khi LARION lại .... làm sao để thuyết phục khách hàng?</p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty. Ví dụ cụ thể: Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</p>', 3, 37, 473, now(), now()),
        (476, 'Demonstrate strong written and verbal communication skills. (Example: formal documents, client / peer or conference presentations).', "<p><strong>Strong</strong>: written &amp; verbal communications là sở trường, lợi thế, sở thích. Được khách hàng, đồng nghiệp, cấp trên khen ngợi, bái phục. Được mọi người tham khảo ý kiến khi cần communicate cho tốt hơn</p><p><em><span style='text-decoration: underline;'><strong>Phản ví dụ:</strong> </span></em>Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty. Ví dụ cụ thể: Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</p>", 3, 37, 474, now(), now()),
        (477, 'Lead or facilitate complex meetings for either internal or external clients. The next level of management likely attends these meetings.', "Example: formal reviews, workshops, requirements sessions", 3, 37, 475, now(), now()),
        (478, 'Display good negotiating skills. (Example: setting scope and time-lines with client, vendor discussions, and technical strategies.)', "<p><span style='text-decoration: underline;'>Ví dụ 1 về good negotiating skills:</span></p><p><span style='text-decoration: underline;'>Khách hàng nói: Dự án phải được hoàn tất trong phạm vi dưới 300 triệu đồng</span><br /><span style='text-decoration: underline;'>Khi dự toán thì thấy do phải mua một số components nên tổng dự toán của dự án là 340 triệu [280 + 60].</span></p><p><span style='text-decoration: underline;'>Nếu thương lượng được để khách hàng đồng ý trả chúng ta 280 triệu còn 60 triệu chi phí mua components khách hàng thanh toán riêng ==&gt; good negotiating skills</span></p><p><span style='text-decoration: underline;'>Ví dụ 2 về good negotiating skills:</span></p><p><span style='text-decoration: underline;'>Khách hàng muốn tiến hành dự án với 3 billable per month in 12 months. Tuy nhiên khách hàng lại muốn trả giá hourly rate thật là thấp, giả sử US$ 8.00.</span></p><p><span style='text-decoration: underline;'>Nếu thương lượng được để khách hàng đồng ý trả chúng ta hourly rate US$ 12.00 với cam kết rằng khối lượng công việc chúng ta hoàn tất được là xứng đáng với hourly rate US$ 12.00 ==&gt; good negotiating skills</span></p><p><span style='text-decoration: underline;'>Ví dụ 3 về good negotiating skills:</span></p><p><span style='text-decoration: underline;'>Một key member muốn rời khỏi công ty, thậm chí đã gửi đơn xin nghỉ việc. Thuyết phục người này ở lại bằng cách nào?</span></p><p><span style='text-decoration: underline;'>Ví dụ 4 về good negotiating skills:</span></p><p><span style='text-decoration: underline;'>Một key member đã rời công ty và đi làm ở công ty khác. Công ty cần người này làm part time vào các thứ bảy, chủ nhật, buổi tối trong một khoảng thời gian. Vậy khoảng thời gian này có tính là overtime với hệ số 1,5 hay không? ==&gt; thương lượng là nó chỉ hệ số 1</span></p>", 4, 37, 476, now(), now()),
        (479, 'Exhibit the appropriate interpersonal skills required to establish effective working relationships with clients and / or business partners', '<p>Thế nào là interpersonal skills? http://en.wikipedia.org/wiki/Interpersonal <br />Nói về Interpersonal Relationship, dựa vào đó sẽ cần có các skills để Flourishing Relationships<br />Hãy chứng minh bạn có Appropriate Interpersonal Skills!</p>', 4, 37, 477, now(), now()),
        (480, 'Demonstrate excellent written and verbal communication skills as well as facilitation skills.', '<p>Không chỉ strong, mà còn phải biết hát. Là một người cực kỳ xuất sắc về communications. Rất hiếm người đạt được slot này.</p><p>Nếu làm việc với khách hàng nước ngoài bằng tiếng Anh thì một điều kiện cần là TOEIC 900?</p>', 4, 37, 478, now(), now()),
        (481, 'Effectively communicate LARION’s capabilities to clients.', '<p>Phải có bằng chứng làm việc với khách hàng bên ngoài – chịu trách nhiệm chính, nhất là trong các dự án mớivới khách hàng bên ngoài.</p><p>Việc chịu trách nhiệm chính và biến dự án tiềm năng thành dự án thực tế cho công ty là bằng chứng rõ ràng.</p>', 4, 37, 479, now(), now()),
        (482, 'Lead or facilitate complex, multi-session, politically charged meetings for clients. The most senior members of the client (internal or external) team are likely to attend these meetings representing their own group’s interest. ', "<p>Chịu trách nhiệm chính từ phía LARION trong các buổi họp phức tạp, khó khăn, gồm nhiều buổi, có yếu tố political [<span style='text-decoration: underline;'><em><strong>ví dụ</strong></em></span> tại sao lại outsource thay vì tự làm; tại sao lại outsource sang VN mà không phải là ở India, etc] liên quan đến khách hàng. Các buổi họp này có đại diện cấp cao nhất của khách hàng tham gia.</p>", 4, 37, 480, now(), now()),
        (483, 'Independently interact with clients and provide guidance and supervision to others in this area ', '<p>Giao tiếp độc lập với khách hàng và hướng dẫn, giám sát, trợ giúp người khác</p>', 4, 37, 481, now(), now()),
        (484, 'Able to improve skill knowledge to adapt yourself to new requirements', '<p>Bằng chứng [bao gồm, không chỉ gồm]:</p><p>Đơn giản nhất là hoàn tất training plan cho newcomer và được đánh giá tốt [đúng hạn, kết quả kiểm tra tốt chẳng hạn]</p><p>Tham gia các khóa huấn luyện của công ty và thi lần đầu là pass ngay</p><p>Nâng cao trình độ Anh văn đúng thời điểm như đã cam kết với công ty</p><p>Tìm hiểu vấn đề mới để giải quyết công việc trong dự án đạt đúng ước lượng hợp lý đã đề ra</p>', 1, 38, 482, now(), now()),
        (485, 'Able to write report if having any request', '<p>Good meeting minutes, các báo cáo về một vấn đề nào đó khi được yêu cầu</p>', 1, 38, 483, now(), now()),
        (486, 'Estimate how much time one has, to allocate it effectively, and to stay within time limits and deadlines.', '<p>Thể hiện ở 2 khía cạnh:</p><p>Với chừng đó thời gian có được thì cần hoàn tất khối lượng công việc tương xứng ==&gt; đây là thứ cần được hỏi và trả lời hàng ngày: <strong>Do I finish a reasonable amount of tasks today? Do the peer / direct manager / customer happy with my achievement?</strong> Nếu thời gian đang được sử dụng không hiệu quả do yếu tố khách quan thì cần có phản hồi với những người có liên quan [ví dụ: suốt ngày đi họp mà lại sử dụng thời gian họp kém hiệu quả…]</p><p>Với chừng đó thời gian có được thì không over-commit để rồi không đạt được commitment</p>', 1, 38, 484, now(), now()),
        (487, 'Arrange and finish tasks creatively ', '', 1, 38, 485, now(), now()),
        (488, 'Complete assigned tasks within scheduled completion dates. Communicate potential issues as soon as they are known.', '', 1, 38, 486, now(), now()),
        (489, 'Prioritize duties in a manner consistent with project objectives / goal', '', 1, 38, 487, now(), now()),
        (490, 'Organize tasks and make an effective plan for own task', '<p>Hoàn tất tasks đúng hạn như ước lượng hợp lý đã đề ra</p>', 2, 38, 488, now(), now()),
        (491, 'Complete critical tasks on time.', '<p>Hoàn tất critical tasks đúng hạn.</p>', 2, 38, 489, now(), now()),
        (492, 'Demonstrate exceptional time management and prioritization skills', '<p>Khả năng quản lý thời gian mà mình có; khả năng đặt độ ưu tiên các tasks.</p><p>Bằng chứng: Hoàn tất tốt công việc khi cùng lúc tham gia nhiều dự án / nhiều việc xen kẽ nhau</p>', 2, 38, 490, now(), now()),
        (493, 'Consistently deliver quality, on-time task assignment outcomes as a result of good planning and organizational skills.', '<p>Nếu có bằng chứng consistently; không có phản ví dụ trong vòng xxx tháng [cả về chất lượng lẫn thời hạn]. Nếu hoàn tất task trong 3 hours nhưng lại tốn thêm 2 hours trở lên để đi sửa ==&gt; phản ví dụ</p>', 2, 38, 491, now(), now()),
        (494, 'Assistant to develop project plans and then tracks tasks, manage scope and risk, accurately reports status to client / project manager / delivery manager.', '<p>Hỗ trợ làm các loại project plans, kiểm soát phạm vi và rủi ro, báo cáo tình trạng dự án cho khách hàng / trưởng dự án / … Ít nhất cũng phải hỗ trợ trong khoảng 3 tháng trở lên</p>', 2, 38, 492, now(), now()),
        (495, 'Successfully complete tasks and assignments independently and supervise the work of others as requested.', '<p>Hoàn tất tasks một cách độc lập [không cần hỏi người khác về cách giải quyết] và có thể kiểm tra, theo dõi, hướng dẫn, hỗ trợ người khác. Bằng chứng có khả năng là phải hoàn tất được tasks một cách độc lập trong khoảng 3 tháng trở lên; hỗ trợ được người khác ít nhất tương ứng 6 billable man-month trở lên</p>', 2, 38, 493, now(), now()),
        (496, 'Develop project plans and then track tasks, manage scope and risk, accurately report status to client / project manager / delivery manager.', '<p>Ít nhất 3 tháng trở lên?</p>', 3, 38, 494, now(), now()),
        (497, 'Develop estimates and schedules for potential follow on opportunities.', '<p>Follow on opportunities: Cho khách hàng tiềm năng. Ít nhất cho 3 khách hàng tiềm năng với tổng số estimated effort ít nhất khoảng 6 billable man-months</p>', 3, 38, 495, now(), now()),
        (498, 'Successfully manage scope and client expectations to deliver Task deliverables that meet and exceed client expectations', '<p>Ít nhất 3 tháng trở lên? Phải có bằng chứng về việc quản lý client expectations và meet / exceed client expectations</p>', 3, 38, 496, now(), now()),
        (499, 'Accountable for accurate and timely feedback to project associates through Task / Documents reviews and / or other forms of formal feedback', '<p>Có phản hồi chính xác và đúng hạn cho các đồng nghiệp / khách hàng hay không?</p>', 3, 38, 497, now(), now()),
        (500, 'Successfully handle and organize strategy task', "<p><span style='text-decoration: underline;'><em><strong>Một số ví dụ:</strong></em></span></p><p>Handle &amp; Organize CMMI Level 3 project</p><p>Handle &amp; Organize Portfolio Management</p><p>Handle &amp; Organize: Quản lý năng suất tại công ty</p><p>Handle &amp; Organize: Quản lý nhân sự tại công ty – bài toán giữ người, bài toán lương thưởng / phúc lợi</p>", 4, 38, 498, now(), now()),
        (501, 'Successfully manage and organize multiple tasks at the company level', '<p>Vừa manage &amp; organize CMMI Level 3 project, vừa quản lý phòng tổng hợp, vừa quản lý Dự án một cách thành công trong tối thiểu 3 tháng chẳng hạn.</p><p>Người đồng thời tham gia nhiều dự án [ví dụ 3 dự án phần mềm] nhưng không phải ở vị trí manager thì sao? Không được tính, chỉ được tính vào slot 2c</p><p>Người đồng thời làm manager ở vài dự án khác nhau thì sao?</p>', 4, 38, 499, now(), now()),
        (502, 'Accountable for developing his / her own management skill set to meet the company expectation. ', '<p>PMP, MBA là điều kiện cần</p><p>Cần có bằng chứng về việc áp dụng management skill set thành công</p>', 4, 38, 500, now(), now()),
        (503, 'Empiricism: apply the concepts of the empirical process to the problems they encounter', '<p>Describe problems in terms of learning, break problems down into the smallest increments that will generate valuable evidence, and execute in an empirical way. By learning and practicing the skills in this Focus Area, a practitioner will become an expert in the application of scientific methods to complex problems, understanding why and how to apply an empirical process.</p>', 1, 39, 501, now(), now()),
        (504, 'Scrum Values: understand both the Scrum Values - Focus, Respect, Openness, Commitment, and Courage', '<p>A practitioner will <strong>understand</strong> both the Scrum Values - Focus, Respect, Openness, Commitment, and Courage - and <strong>demonstrate</strong> that they can apply them in the reality of organizations.</p>', 1, 39, 502, now(), now()),
        (505, 'Roles: understand the roles – Product Owner, Scrum Master, and Development Team Member, their responsibilities', '<p>The practitioner will <strong>understand</strong> the roles, their responsibilities, and also how to instantiate these roles in existing or new job titles. They will be able to describe the implications of these roles as they apply to existing HR practices and as they apply to self-organization that is reinforced by the role separation.</p>', 1, 39, 503, now(), now()),
        (506, 'Events: understand the events - The Sprint, Sprint Planning, Daily Scrum, Sprint Review, and the Sprint Retrospective, and be able to practice each event', '<p>Facilitate events in time-boxed and enable progress through adaptation and transparency.</p>', 1, 39, 504, now(), now()),
        (507, 'Artifacts: understand these artifacts - The Product Backlog, Sprint Backlog, and Increment', '<p>The Practitioner will <strong>understand</strong> these artifacts and how to <strong>implement</strong> them in complex, real-world situations.</p>', 1, 39, 505, now(), now()),
        (508, "Done: understand how the DoD can be visualized and communicated within the organization", '<p>The practitioner will be able to describe what a DoD is, apply it to their particular context, and understand how the DoD can be visualized and communicated within the organization.</p>', 1, 39, 506, now(), now()),
        (509, 'Scrum Values: understand both the Scrum Values - Focus, Respect, Openness, Commitment, and Courage', '<p>Help others to apply them to create an environment where empirical process, self- organization, and continual improvement will be more successful.</p>', 2, 39, 507, now(), now()),
        (510, 'Roles: understand the roles – Product Owner, Scrum Master, and Development Team Member, their responsibilities', '<p>Describe the implications of these roles as they apply to existing HR practices and as they apply to self-organization that is reinforced by the role separation.</p>', 2, 39, 508, now(), now()),
        (511, 'Artifacts: understand these artifacts - The Product Backlog, Sprint Backlog, and Increment', "<p>• Understand the relationship of these artifacts relative to other practices and techniques<br />• Integrate them into an organization's own process.</p>", 2, 39, 509, now(), now()),
        (512, "Done: understand how the DoD can be visualized and communicated within the organization", '<p>• Describe the implications of the necessary trade-offs and compromises required to deliver “Done” Product Increments within their organization</p>', 2, 39, 510, now(), now()),
        (513, 'Scaling: apply Scrum in increasing levels of complexity and scale.', '<p>• Demonstrate when to scale and when not to scale and appreciate scaling practices and complementary frameworks that help organizations scale Scrum. <br />• Know what, and what not, to compromise in pursuit of a scaling approach by understanding the trade-offs and benefits of particular concepts and practices. <br />• Demonstrate that they can scale Scrum and still keep its essential qualities of empiricism, self-organization, and continuous improvement. <br />• Should also be able to demonstrate the results of good scaling practices from both an organization and business perspective</p>', 3, 39, 511, now(), now()),
        (514, 'Self-organizing team: understand what self-organization is and how to apply it to their context.', '<p>• Understand what self-organization is<br />• How to apply it to their context.</p>', 1, 40, 512, now(), now()),
        (515, 'Facilitation: apply a set of practices that help support the collaboration, communication, and creativity of teams and individuals.', '<p>• Understand the value of facilitation<br />• Have a collection of techniques they can apply. <br />• Experience applying them in different situations with medium levels of complexity.</p>', 1, 40, 513, now(), now()),
        (516, "Leadership styles: Understanding the right leadership style - traditional ‘command and control’ to more collaborative or even Machiavellian, to use at a given time .", '<p>• Understand the concepts of leadership styles and be able to apply a particular style when the situation calls for it.</p>', 1, 40, 514, now(), now()),
        (517, 'Self-organizing team: understand what self-organization is and how to apply it to their context.', '<p>• Understand how to incrementally introduce self-organization, the practices that can help self-organization occur</p>', 2, 40, 515, now(), now()),
        (518, 'Leadership styles: Understanding the right leadership style - traditional ‘command and control’ to more collaborative or even Machiavellian, to use at a given time .', '<p>• Demonstrate their ability to decide on the right style and understand its impact on the organization.</p>', 2, 40, 516, now(), now()),
        (519, 'Coaching and Mentoring: help people get better at their work, deliver more value, or resolve a conflict or problem', '<p>• Coach as well as mentor. <br />• Understand different formal coaching techniques and be able to apply those techniques in different complex situations.</p>', 2, 40, 517, now(), now()),
        (520, 'Self-organizing team: understand what self-organization is and how to apply it to their context.', '<p>• Apply the measures to judge if a team is able to be empowered to self-organize.</p>', 3, 40, 518, now(), now()),
        (521, 'Forecasting and Release Planning: Complex problems and the application of an empirical process requires a specific way of planning, estimating, and forecasting. ', '<p>• apply agile forecasting and release planning techniques, and understand the value of different approaches.</p>', 1, 41, 519, now(), now()),
        (522, 'Product vision defines the purpose or goal that the product serves, and is defined by the “value” that the product strives to deliver', '<p>• describe what a product vision is <br />• apply what techniques should be employed to both build a vision and make it transparent.</p>', 1, 41, 520, now(), now()),
        (523, 'Product Value: value is complex, made up of long-term and short-term impact, internal and external value, and indirect and direct value', "<p>• understand how to define value for context, and apply it to the work they and the team do<br />• manage others' understanding of value and apply different techniques and practices for defining, communicating and measuring value.</p>", 1, 41, 521, now(), now()),
        (524, 'Product Backlog Management: provides transparency into what is happening to the product for the team, organization, and stakeholders', '<p>• understand how to make the Product Backlog transparent<br />• understand how to manage stakeholder expectations associated with the backlog.</p>', 1, 41, 522, now(), now()),
        (525, 'Forecasting and Release Planning: Complex problems and the application of an empirical process requires a specific way of planning, estimating, and forecasting. ', '<p>• understand which approaches work better in different situations. <br />• understand how releases should be planned while dealing with complexity, dependencies, and value creation.</p>', 2, 41, 523, now(), now()),
        (526, 'Product vision defines the purpose or goal that the product serves, and is defined by the “value” that the product strives to deliver', '<p>• Use a Product Vision to drive strategy and execution<br />• Build a vision that motivates, communicates, and provides constraints for delivery.</p>', 2, 41, 524, now(), now()),
        (527, 'Product Value: value is complex, made up of long-term and short-term impact, internal and external value, and indirect and direct value', '<p>• understand the connection between value and empirical process<br />• understand how value should be the driving factor of the Product Vision.</p>', 2, 41, 525, now(), now()),
        (528, 'Emergent Software Development: describe what emergent architecture is and how it translates into incremental development and delivery', '<p>• describe practices that “realize” the architecture incrementally into a working, agile system. <br />• understand the trade-offs between value, flexibility, and quality<br />• apply techniques that make the emergent approach transparent to the team, organization, and stakeholders</p>', 1, 42, 526, now(), now()),
        (529, 'Managing Technical Risk: describes how technical risks are managed within an Agile approach.', '<p>• understand what technical risks are and how to effectively manage them in an empirical process.<br />• apply practices to make risks transparent.</p>', 1, 42, 527, now(), now()),
        (530, "Continuous Quality: describes what quality is and how the ideas of Agility and Scrum change a product's quality approach", '<p>• understand what continuous quality is<br />• apply continuous quality and the appropriate practices for delivering quality in a continuous way. <br />• understand important concepts like technical debt, Test Left, and the ideas of user-driven testing.</p>', 1, 42, 528, now(), now()),
        (531, 'Emergent Software Development: describe what emergent architecture is and how it translates into incremental development and delivery', '<p>• apply techniques that make the emergent approach transparent to the team, organization, and stakeholders</p>', 2, 42, 529, now(), now()),
        (532, 'Organizational Design and Structure', '<p>• describes the fundamental differences of an agile organization; namely its structure, culture, and design.</p>', 1, 43, 530, now(), now()),
        (533, 'Organizational Design and Structure: describes the fundamental differences of an agile organization; namely its structure, culture, and design.', '<p>• understand approaches for implementing the agile enterprise in a traditional organization.</p>', 2, 43, 531, now(), now()),
        (534, 'Organizational Design and Structure: describes the fundamental differences of an agile organization; namely its structure, culture, and design.', '<p>• Balance the needs for agility with the existing reality of traditional organizational structures.<br />• Transform traditional organization into agile organization</p>', 3, 43, 532, now(), now());
