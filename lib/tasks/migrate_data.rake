      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(547, 'Identify and solve simple problems independently (for example: recognizing changes in scope and communicating them).', 'Không cần hỏi người khác về giải pháp cho vấn đề [đơn giản] đang gặp phải', 2, 45, 13);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(548, 'Provide assistance in solving complex problems (complex – technically difficult, opposing viewpoints, risky, and/or sensitive).', 'Đề xuất, hỗ trợ được trong việc giải quyết các vấn đề phức tạp', 2, 45, 14);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(549, 'Lead simple meetings for internal or external clients. Peer and/or next level management are likely to attend these meetings.', 'Bằng chứng thuyết phục là có dính dáng đến khách hàng bên ngoài', 2, 45, 15);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(550, 'Possess and use good diagnosis / troubleshooting skills.', '', 2, 45, 16);
