        (2462, '- đối với các task của mss trong thời gian đầu quay lại dự án ( wi liên quan đến mhub và ahub), thực hiện việc liên lạc với khách hàng để lấy requirement và distribute cho team đầy đủ, kịp thời để không khiến cho việc phát triển bị ảnh hưởng.\n\n- đối với dự án vendor portal, không tốn quá nhiều thời gian để tìm hiểu về business và làm quen với công việc của dự án', 3, true, '', 2462, now(), now(), false),
        (2463, 'Bên cạnh việc làm những task của ba, vì vấn đề của dự án mà đã có cơ hội để thực hiện công việc qc, có đóng góp để hỗ trợ team trong việc đảm bảo chất lượng trước khi deliver sản phẩm.', 4, true, '', 2463, now(), now(), false),
        (2464, 'Luôn có danh sách những task cần làm với độ ưu tiên đi kèm, và thực hiện các task theo kịp deadline ( tài liệu prototype, tài liệu requirement specs)', 3, true, '', 2464, now(), now(), false),
        (2465, '- tuân thủ quy trình làm và tài liệu: \n+ biết cách tạo và đánh version của tài liệu\n+ xin approve tài liệu trược hoặc trong build cuối.\n\n- không commit tài liệu lên thư mục chính của dự án, vì đây là task của những người có quyền cc', 4, true, '', 2465, now(), now(), false),
        (2466, 'Đã pass ở kỳ trước', 3, true, '', 2466, now(), now(), false),
        (2467, 'Đóng góp trong việc giúp đỡ ba cùng team trong việc review requirement cũng như các tài liệu liên quan. giúp cho việc phát triển sản phẩm của dự án đạt chất lượng', 3, true, '', 2467, now(), now(), false),
        (2468, '- meeting với khách hàng để lấy feedback và confirm requirement\n\n- distribute và discuss với team về requirement, nếu như team có thắc mắc, hay có gặp khó khăn ảnh hưởng đến tiến độ công việc thì phải làm việc với khách hàng để đảm bảo rằng tính năng release sẽ vừa đủ, và đáp ứng được nhu cầu của khách hàng', 4, true, '', 2468, now(), now(), false),
        (2469, 'Thực hiện việc mentor cho haptt của dự án vendor portal.', 3, true, '', 2469, now(), now(), false),
        (2470, '- mặc dù không phải là người nắm quá vững, nhưng khi thực hiện wi hay về module nào mà mình đã đang và sẽ làm, tôi sẽ tìm hiểu kỹ về module đó để đảm bảo hiểu đúng và có thể giái đáp thắc mắc của team về vấn đề.\n\nvd: khi làm về favorite list, cố gắng tìm hổi cội nguồn và những tính năng liên quan, để khi thực hiện, hoặc là sau này khi những bạn ba khác thực hiện các tính năng liên quan có thể sang tham khảo.\n\nvd khi thực hiện dự án vendor portal, đọc về các thông tin trước khi vào dự án để khi làm có thể nắm rõ', 3, true, '', 2470, now(), now(), false),
        (2471, '- đối với các công việc của cá nhân, luôn có check-list để keep track việc thực hiện task của mình. không làm phiền và phải để người khác thúc giục về tiến độ. nếu gặp vấn đề khó khăn sẽ cố gắng tự research trước khi hỏi .\n\n- có thể hỗ trợ và hướng dẫn haptt trong việc phân tích requirement, cải thiện khả năng tiếp cận vấn đề', 3, true, '', 2471, now(), now(), false),
        (2472, 'Trong quá trình làm việc cho dự án scs, đóng vai trò sm nhưng cũng là ba, đưa ra những suggestion cho phía kh để improve ui/ux cũng như thuyết phục khách hàng sử dụng solution khác mà phía bên dự án có thể thấy tốt hơn là solution của kh đưa ra.\nvd: như việc thực hiện ticket liên quan đến challenges header và challenge ui, expect của kh khi discuss với team thấy không tốt và sẽ take time của cả dev lẫn phía user nên đã chủ động thực hiện việc update ui và gợi ý cho kh.', 3, true, '', 2472, now(), now(), false),
        (2473, 'Khi gặp issue, hoặc khi khác hàng gặp issue, cố gắng trong việc tìm hiểu rootcause, và liên lạc với dev có khả năng, có kinh nghiệm để tìm cách xử lý\n\n24032020 – follow up issue bằng việc test để reproduce issue từ phía kh gặp phải, research về issue và đưa thông tin cho phía kh. khi có debugging session, chủ động tham gia để nắm được tình hình và cung cấp thông tin cho phía kh', 4, true, '', 2473, now(), now(), false),
        (2474, 'Các vấn đề đơn giản đều có thể tự đưa ra hướng giải quyết, và solution. đến bước cuối cùng trước khi implement mới nhờ phia  po review để đảm bảo rằng tính năng và cách phân tích của mình sẽ không ảnh hưởng luồng đi, và đảm bảo tính đồng nhất', 3, true, '', 2474, now(), now(), false),
        (2475, '- tham gia trong các buổi grooming của dự án mims và tích cực đóng góp ý kiến khi team có vướng mắc về độ phức tạp của vấn đề và requirement.\n- ở dự án scs, tham gia vào việc đề xuất kh cho những vấn đề khó mà phương án từ phía kh chưa nghĩ tới', 3, true, '', 2475, now(), now(), false),
        (2476, 'Ở quy mô internal:\n- lead meeting grooming của team mims, - - request và take lead các meeting planning/ grooming của dự án scs\n\nở quy mô external:\n- thực hiện việc request các meeting về requirement review với kh và debugging session, cũng như các meeting adhocs từ phía kh, sẽ là người tìm người và tổ chức họp, và  trước/ sau các buổi họp sẽ summarize về nội dung sẽ/ đã được trao đổi của buổi họp cũng như định hướng trong thời gian tới đối với các vấn đễ đã được thảo luận.', 3, true, '', 2476, now(), now(), false),
        (2477, '- khi làm việc, thực hiện việc testing và khi gặp bug/ issue sẽ chủ động biết được nguyên nhân sơ bộ của vấn đề để có thể tiến hành trao đổi với dev.\n- sử dụng inspect ( f12) để kiểm tra khi có vấn đề về giao diện cũng như api.\n- có thể sử dụng inspect tool để thực hiện việc tìm hiểu những design/ component có sẵn của sản phẩm đang như nào để đưa ra được phương án xử lý cũng như proposal thiết kêt.', 3, true, '', 2477, now(), now(), false),
        (2478, 'Đã và đang thực hiện trong nội bộ dự án scs, còn đối với dự án vp thì vẫn cần hỏi ý kiến và sự support từ po', 2, true, '', 2478, now(), now(), false),
        (2479, '- đối với các vấn đề lớn của dự án scs, hiện tại đang thực hiện việc chia nhỏ và chia vùng để có quản lý tốt hơn và assign người làm cùng nhau để đảm bảo vấn đề có thể được xử lý nhiều người cùng nhau.\n\n- đối với 1 số issue mà phía team không trực tiếp gây nên nhưng kh gặp lỗi, đã cố gắng trong việc cố gắng reproduce, xin source, lấy môi trường để reproduce issue, hiểu sơ bộ nguyên nhân trước khi đưa tới cho team dev xử lý. trong quá trình team dev xử lý thì sẽ follow cùng team để thực hiện việc communicate với kh và giải thích những vướng mắc về mặt requirement cũng như process.', 3, true, '', 2479, now(), now(), false),
        (2480, '- viết email cũng như trao đổi trực tiếp với khách hàng về những vấn đề còn khúc mắc và cần giải quyết. từ đó chuyển thành những tài liệu dưới dạng text để team có thể download về, mở ra và follow theo.\n\n- khi team member có bất cứ issue gì về requirement hay những điều khó hiểu, có thể đứng ra giải thích cũng như giúp team hiểu rõ vấn đề hơn trực tiếp trong các buổi grooming, planning hoặc trao đổi riêng,', 4, true, '', 2480, now(), now(), false),
        (2481, 'Các buổi meeting ở cấp độ dự án nhỏ ( vendor portal), dự án mss, ba team cũng như các meeting với khách hàng đều tham gia đầy đủ và đúng giờ.', 4, true, '', 2481, now(), now(), false),
        (2482, '- lead grooming của team vendor portal ( grooming, planning):\n+ chuẩn bị máy móc ( máy tính, điều hòa) – mọi thứ sẵn sàng và mát mẻ cho team vào có 1 buổi họp hiêu quả \n+ chuẩn bị tài liệu đầy đủ trước khi meeting \n\n- lead 1 buổi thuyết trình của team po/ ba – chủ động về thời gian cũng như nội dung của buổi thuyết trình', 3, true, '', 2482, now(), now(), false),
        (2483, 'Thường xuyên có những buổi meeting với khách hàng để trao đổi về requirement cũng như support khách hàng trong việc xây dựng demo script trước những buổi demo của khách hàng bằng cách trao đổi trực tiếp ( qua skype chat, skype call và email). \n\n24032020 – khi làm việc ở dự án scs, đã có thể chủ động và tự mình sắp xếp cũng như take lead/ controll các buổi meeting với kh ( không chỉ là daily standup mà còn là các buổi meeting liên quan đến requirement discussion and issue/ bugs discussion).', 4, true, '', 2483, now(), now(), false),
        (2484, 'Có khả năng viết và nói trôi chảy, không bị vấn đề “nói mà khách hàng không hiểu”. trong email, thi thoảng còn viết hơi thảo mai. \n\nngoài ra, đôi khi còn support được anh cường trong viêc chọn lựa câu chữ để sao cho thảo mai và phù hợp khi viết mail cho khách hàng và làm ui :d\n\n24032020 – khi thực hiện công tác ở dự án scs, là người giao tiếp chủ yếu với kh, khi meeting, có những vấn đề mà ba khác giải thích kh không hiểu rõ thì sẽ là người giải thích lại cho kh và giúp đỡ team cũng như kh trong việc hiểu được ý và giúp cho công việc phát triển tốt hơn.', 4, true, '', 2484, now(), now(), false),
        (2485, '- trước đây đã thực hiện việc hướng dẫn cho haptt nhưng mà chưa đạt được kết quả tốt lắm ( do cách hướng dẫn chưa tốt và cần được cải thiện)\n- ở dự án scs, khi member mới vào ( dev) thực hiện việc transfer knowledge cho bạn về dự án và giúp bạn hiểu được dự án đang chạy như nào và biusiness hiện tại như nào. tuy nhiên, đối với ba mới vào dự án, hiện tại chưa thực hiện được việc transfer knowledge tốt lắm và cũng cần được cải thiện.', 2, true, '', 2485, now(), now(), false),
        (2486, 'Với slot này em không biết nên input thông tin gì và chưa biết mình đã thực hiện được chưa nên hiện tại để là “does not meet minimum standards” ạ', 1, true, '', 2486, now(), now(), false),
        (2487, '- trong quá trình review tài liệu cho haptt, thường xuyên thực hiện việc kiểm tra lỗi chính tả, lỗi câu cũng như cách diễn đạt trong requirement sao cho dễ hiểu. chỉnh sửa email gửi khách hàng của haptt\n\n- khi meeting với khách hàng, không gặp quá khó khăn để hiểu cũng như để khách hàng hiểu câu hỏi và yêu cầu mà mình cần được làm rõ \n\n- thi thoảng có thể giúp đỡ các team member khác trong việc chọn từ cũng như biểu đạt thành câu', 3, true, '', 2487, now(), now(), false),
        (2488, 'Trong quá trình làm việc cho dự án scs, là người call và facilitate các buổi meeting, bao gồm:\n- retro meeting với team và retro meeting với khách hàng phía scs\n- daily standup meeting\n- requirement discussion and debug session – take lead trong việc thực hiện meeting và take notes, distribute lại cho team về các thông tin trong meeting và follow up issue/ phân bố sắp xếp resource để thực hiện các issue/ discussion đã được thảo luận trong meeting.', 4, true, '', 2488, now(), now(), false),
        (2489, '- đàm phán với kh về số lượng các tickets sẽ được làm trong từng sprint ( nếu trường hợp thời gian không đủ hoặc team có issue).\n- ngoài ra, chủ động tiến hành đàm phán với kh về việc extend thời gian để thực hiện tickets cũng như sprint khi phát hiện ra những impact thực sự mà team chưa lường tới.', 3, true, '', 2489, now(), now(), false),
        (2490, 'Các kỹ năng mềm của bản thân tự đánh giá là có và ở mức vừa phải, không quá tốt nhưng đủ dùng để làm việc cũng như giao tiếp với phía kh và quản lý công việc', 3, true, '', 2490, now(), now(), false),
        (2491, 'Nếu so với điều kiện ở cột c thì em cảm thấy em có thể pass :d \n- biết hát\n- và điểm toeic hồi trước của em là 910 :d\n\ntuy nhiên, bằng chứng chi tiết cho slot này thì em đang chưa nắm rõ để có thể đánh thêm', 3, true, '', 2491, now(), now(), false),
        (2492, '- tham gia khóa training người mới, isms\n\n- tham gia training agile/ scrum\n\n- tham gia các lớp training của dự án và nắm được nội dung training', 3, true, '', 2492, now(), now(), false),
        (2493, '- viết meeting minutes cho team 3, team  vendor portal', 3, true, '', 2493, now(), now(), false),
        (2494, "Việc thực hiện task một cách sáng tạo, với cá nhân em thì em không biết nên định nghĩa thế nào là sáng tạo. do đó ở slot này, em sẽ chỉ đơn thuần là để vào cách mà em thực hiện các task mà em nghĩ là hợp lý.\n\n+ vào cuối mỗi ngày, rà soát và đánh giá công việc mà mình đã làm trong ngày. những việc nào còn tồn đọng sẽ được kéo sang cột to-do ở trên trello\n+ sáng hôm sau, sẽ list ra danh sách những công việc cần làm trong ngày. dành ra khoảng 5-10\\' để đánh giá độ ưu tiên. những công việc còn tồn đọng từ ngày hôm trước sẽ có độ ưu tiên cao hơn những công việc mới được list ra ngày hôm nay. tuy nhiên, không chỉ là việc thời gian của task, mà còn là priority của task cũng được đánh giá.\n+ những task nào cần chú ý thì sẽ được highlight màu đỏ ở trước, và đi kèm 1 số ký hiệu để dễ nhìn và dễ chú ý.", 3, true, '', 2494, now(), now(), false),
        (2495, "Việc thực hiện task một cách sáng tạo, với cá nhân em thì em không biết nên định nghĩa thế nào là sáng tạo. do đó ở slot này, em sẽ chỉ đơn thuần là để vào cách mà em thực hiện các task mà em nghĩ là hợp lý.\n\n+ vào cuối mỗi ngày, rà soát và đánh giá công việc mà mình đã làm trong ngày. những việc nào còn tồn đọng sẽ được kéo sang cột to-do ở trên trello\n+ sáng hôm sau, sẽ list ra danh sách những công việc cần làm trong ngày. dành ra khoảng 5-10\\' để đánh giá độ ưu tiên. những công việc còn tồn đọng từ ngày hôm trước sẽ có độ ưu tiên cao hơn những công việc mới được list ra ngày hôm nay. tuy nhiên, không chỉ là việc thời gian của task, mà còn là priority của task cũng được đánh giá.\n+ những task nào cần chú ý thì sẽ được highlight màu đỏ ở trước, và đi kèm 1 số ký hiệu để dễ nhìn và dễ chú ý.", 3, true, '', 2495, now(), now(), false),
        (2496, '- đối với những task của ba, thực hiện việc communicate và confirm requirement ontime. bên cạnh đó, về phía quy trình của công ty, các tài liệu của ba ( bao gồm ui và requirement specification) luôn được thực hiện và xin approve vào đúng hạn.\n\n- trong thời gian team vendor portal không có qc, ba phải takes care thêm các task về test, mặc dù áp lực và khối lượng công việc nhiều hơn nhưng vẫn cố gắng hoàn thành để đảm bảo ngày release. khi gặp vấn đề về việc không đủ thời gian test, do vấn đề về ngày ra build của devs, đã ngay lập tức report lên po để request thời gian ot, để test và đảm bảo sản phẩm được test qua hết trước khi release.', 3, true, '', 2496, now(), now(), false),
        (2497, 'Khi thực hiện wi cho 2 dự án mss và vendor portal, vì wi liên quan đến ahub-mhub có priority cao hơn nên sắp xếp thời gian để confirm và làm task đó trước. rồi sau đó, dành thời gian trong ngày làm task phía bên vendor portal để đảm bảo rằng không task bên nào bị lãng quên cả \n\nngoài ra, đối với những task nội bộ của vendor portal, đối với những task mà độ cần thiết cũng như yêu cầu gấp từ phía khách hàng sẽ được sắp xếp và ưu tiên làm trước. bên cạnh đó, các task khác cũng sẽ được chọn lựa để làm trong sprint để đảm bảo mặc dù đáp ứng nhu cầu của khách hàng nhưng việc phát triển của team cũng sẽ tiến hành như dự định.', 3, true, '', 2497, now(), now(), false),
        (2498, '- đối với các task cá nhân, luôn có plan cũng như cách quản lý dể các công việc của mình hoàn thành đúng hạn, không làm ảnh hưởng đến tiến độ của các thành viên khác trong team', 3, true, '', 2498, now(), now(), false),
        (2499, 'Một số task critical đối với bản thân em:\n- thực hiện việc thiết kế ui và lấy confirm về requirement đối với khách hàng – thực hiện việc lấy confirm từ phía khách hàng ontime, không để cho việc đợi confirm từ phía khách hàng quá lâu gây ảnh hưởng đến tiến độ phát triển sản phẩm của team\n\n- communicate với khách hàng – về những câu hỏi cùa bản thân cũng như những vấn đề của team gặp phải trong quá trình thực hiện.', 3, true, '', 2499, now(), now(), false),
        (2500, '- đối với công việc hàng tuần, và hàng sprint, luôn có 2 check-list để cá nhân có thể keep track được:\n+ 1 bản ở trên hệ thống trello – giúp quản lý các task ở công ty, liên quan đến dự án để đảm bảo tiến độ của bản thân không bị chậm, từ đó sẽ không làm ảnh hưởng đến tiến độ của team\n\n+ 1 bản ở trong sổ tay cá nhân – bao gồm cả những công việc của dự án cũng như công việc của cá nhân, để đảm bảo việc không quá tập trung vào công việc mà bỏ quên thời gian dành cho bản thân, cũng như việc không để thời gian dành cho bản thân nhiều quá mà ảnh hưởng đến công việc và thời gian của dự án.\n\n- thực hiện công việc của bản thân, công việc mentor cũng như của dự án sales and marketing mà vẫn đảm bảo rằng không công việc của bên nào bị ảnh hưởng', 3, true, '', 2500, now(), now(), false),
        (2501, 'Hoàn thành task đúng thời hạn. không để quản lý cấp trên phải nhắc nhở về deadline', 3, true, '', 2501, now(), now(), false),
        (2502, 'Hiện tại đã và đang thực hiện việc quản lý dự án và rủi ro, báo cáo tình trạng của dự án và issue đến cho kh và cấp trên khi có issue xảy ra ( issue từ phía kh report cũng như issue từ phía team report lên).\n\ntuy nhiên, tính đến thời điểm hiện tại thì chỉ mới thực hiện được trong 2 tháng, chưa đủ 3 tháng nên cần thêm thời gian để học tập và cải thiện thêm.', 2, true, '', 2502, now(), now(), false),
        (2503, '- hoàn thành công việc ba của team 3 cũng như team vendor portal độc lập.\n\n- support ba và qc trong việc hiểu requirement, phân tích requirement và chỉnh sửa các lỗi sai\n\n24032020 – thực hiện việc xử lý issue với kh độc lập, song song với đó là hướng dẫn ba khác trong dự án scs về issue hiện tại và phương hướng giải quyết cho những issue mà người đó gặp phải.', 4, true, '', 2503, now(), now(), false),
        (2504, 'Hiện tại đã và đang thực hiện việc quản lý dự án và rủi ro, báo cáo tình trạng của dự án và issue đến cho kh và cấp trên khi có issue xảy ra ( issue từ phía kh report cũng như issue từ phía team report lên).\n\ntuy nhiên, tính đến thời điểm hiện tại thì chỉ mới thực hiện được trong 2 tháng, chưa đủ 3 tháng nên cần thêm thời gian để học tập và cải thiện thêm.', 2, true, '', 2504, now(), now(), false),
        (2505, 'Chưa có cơ hội thực hiện', 1, true, '', 2505, now(), now(), false),
        (2506, 'Quản lý việc sắp xếp requirement và expectation của kh, đảm bảo cho việc các requirement sẽ được thực hiện đúng như khách hàng mong đợi. \n\ncác yêu cầu của kh bị thay đổi liên tục nên luôn cần phải theo dõi sát sao yêu cầu của khách hàng.\n\ntuy nhiên, tính đến thời điểm hiện tại thì chỉ mới thực hiện được trong 2 tháng, chưa đủ 3 tháng nên cần thêm thời gian để học tập và cải thiện thêm.', 2, true, '', 2506, now(), now(), false),
        (2507, '- đối với những thắc mắc về requirement của member trong team, luôn cố gắng để giải đáp 1 cách nhanh chóng và sớm nhất có thể để không làm ảnh hưởng đến thời gian cũng như tiến độ của team.\n\n- đối với những yêu cầu mang tính chất “cần phản hồi” từ phía khách hàng, luôn cố gắng trả lời ngay khi tìm hiểu được. ngoài ra, với những request support từ phía michele trong việc “play arounf” với vendor portal thì luôn luôn phản hồi ngay khi khách hàng hỏi, kể cả ngoài giờ làm việc.\n\n- đối với những tài liệu cần review, luôn cố gắng sắp xếp thời gian để review và đưa phản hồi cho các bạn sớm để các bạn chỉnh sửa ( nếu cần)', 3, true, '', 2507, now(), now(), false),
        (2508, 'Brainstorming\ndocument analysis\nfocus groups\ninterface analysis\ninterviews\nobservation\nprototyping\nrequirements workshops\nsurvey/questionnaire', 3, true, '', 2508, now(), now(), false),
        (2509, '- làm việc với khách hàng để collect requirement, các file review cũng như flow.\n\n- sắp xếp và chuẩn bị danh sách câu hỏi liên quan đến requirement để lấy được feedback một cách nhanh chóng và dễ dàng, tiết kiệm thời gian cho 2 bên.\n\n- khi có vấn đề cần làm rõ hoặc cần khách hàng, chủ động liên hệ để có thể trao đổi ( bằng email, bằng chat qua skype, skype call)', 3, true, '', 2509, now(), now(), false),
        (2510, '- brainstorming \n\n- document analysis – đưa ra những tài liệu cá nhân và 1 số mẫu tài liệu tìm hiểu trên mạng để có thể support đồng nghiệp trong việc phát triển cá nhân\n\n- survey/questionnaire – support haptt trong việc làm rõ requirement và chuẩn bị danh sách câu hỏi để confirm với khách hàng', 3, true, '', 2510, now(), now(), false),
        (2511, 'Được yêu cầu để thực hiện việc support trong việc phân tích requirement và brainstorming về những wi sẽ làm trong thời gian tới, và đưa ra những gợi ý, câu hỏi cho ba khác trong dự án.', 3, true, '', 2511, now(), now(), false),
        (2512, 'Brainstorming\ndocument analysis\nfocus groups\ninterface analysis\ninterviews\nobservation\nprototyping\nrequirements workshops\nsurvey/questionnaire', 3, true, '', 2512, now(), now(), false),
        (2513, '- brainstorming\n\n- document analysis\n\n- prototyping\n\n- survey/questionnaire – trước mỗi buổi meeting đều chuẩn bị danh sách những câu hỏi cần hỏi khách hàng để giúp cho việc lấy requirement được tốt hơn. bên cạnh đó, danh sách các câu hỏi được phát sinh trong quá trình phân tích requirement cũng được keep track ở file cùng với backlog', 3, true, '', 2513, now(), now(), false),
        (2514, '- brainstorming\n\n- document analysis\n\n- prototyping – phát triển ui trên powperpoint. không chỉ prototyping mà còn thực hiện việc vẽ các diagram để có thể làm rõ thêm\n\n- survey/questionnaire – trước mỗi buổi meeting đều chuẩn bị danh sách những câu hỏi cần hỏi khách hàng để giúp cho việc lấy requirement được tốt hơn. bên cạnh đó, danh sách các câu hỏi được phát sinh trong quá trình phân tích requirement cũng được keep track ở file cùng với backlog', 3, true, '', 2514, now(), now(), false),
        (2515, 'Vendor portal là dự án được xây dựng mới hoàn toàn dựa trên những mong muốn của khách hàng. trong quá trình phát triển, càng về sau thì những requirement bị thay đổi nhiều và càng trở nên phức tạp, và requirement cũng bị conflict rất nhiều so với những gì hiện có. \n\nvới vai trò là ba, khi thực hiện tiếp nhận requirement và phân tích requirement, nhận định được những conflic requirement đối với sản phẩm hiện tại, tôi đã thực hiện phân tích và thông báo đến team. trong trường hợp ảnh hưởng lớn đến cấu truc và thiết kế code cũng như database, thì sẽ làm việc với các dev cũng như tech-lead để thu thập thông tin, define chi tiết và làm việc với khách hàng để tìm ra hướng giải quyết tốt nhất.\n\ncó những wi khách hàng mong muốn thực hiện sớm so với plan thì tôi đã cùng team phân tích và đánh giá mức độ ảnh hưởng. trong trường hợp những wi cần làm ngay nhưng chưa có thông tin hoặc mức độ ảnh hưởng lớn thì sẽ liệt kê ra những bắng chứng và thông tin đó để đi nói chuyện với khách hàng.', 3, true, '', 2515, now(), now(), false),
        (2516, '- thực hiện phân tích vendor portal với nhiều module con trực thuộc \n\n- thực hiện research về các hệ thống đã có sẵn và tham khảo để phát triển hệ thống một cách thân thiện và gần gũi với người dùng hơn', 3, true, '', 2516, now(), now(), false),
        (2517, '- sau khi nhận requirement từ phía ba/ po và nhận được hướng dẫn cơ bản về cdb, có thể thực hiện được việc research và làm việc với dự án khác về kiến thức để thực hiện được wi.\n\n- ngoài ra những wi khác có thể thực hiện research và phân tích sau khi được chỉ dẫn', 3, true, '', 2517, now(), now(), false),
        (2518, 'Prioritization techniques\n- ranking. ...\n- numerical assignment (grouping) ...\n- moscow technique. ...\n- bubble sort technique. ...\n- hundred dollar method. ...\n- analytic hierarchy process (ahp) ...\n- five whys.\n\nrequirement organization/ allocation\n- traceability\n- requirements change management\n- investigation\n- feasibility', 3, true, '', 2518, now(), now(), false),
        (2519, '- ranking technique - \n\n- moscow technique\n+ must (mandatory)\n+ should (of high priority)\n+ could (preferred but not necessary)\n+ would (can be postponed and suggested for future execution)\n\n- bubble sort technique\n\n- lean prioritization – to define the priority ò wi by define priority x urgent, or value x cost', 3, true, '', 2519, now(), now(), false),
        (2520, '- sử dụng requirement traceability matrix', 3, true, '', 2520, now(), now(), false),
        (2521, 'Đã pass ở kỳ trước', 3, true, '', 2521, now(), now(), false),
        (2522, 'Đã pass ở kỳ trước', 3, true, '', 2522, now(), now(), false),
        (2523, 'Đã pass ở kỳ trước', 3, true, '', 2523, now(), now(), false),
        (2524, '- đối với những solution được đưa ra bởi ba của dự án scs sau khi review thì sẽ discuss và đề xuất những thay đổi để có thể gia tăng giá trị của solution cũng như hạn chế được những thiếu sót của team trong quá trình thực hiện implement.\n\n- đề xuất với kh những thay đổi về ui/ux của hệ thống nên có dựa trên kinh nghiệm cũng như các trải nghiệm của bản thân khi sử dụng những hệ thống khác', 3, true, '', 2524, now(), now(), false),
        (2525, 'Trong quá trình phân tích và phát triển requirement cho dự án vendor portal cũng như dự án mss, các techniques sau đây đã được áp dụng cho bản thân, và support cho cả đồng nghiệp:\n\n- acceptance criteria – được liệt kê ra ở đầu mỗi file ui prototype – liệt kê ra những điều kiện cần được thỏa mãn, những tính năng cần phải có khi phát triển 1 wi. nếu những điều kiện này được thỏa mãn thì wi mới được xem xét là thông qua, còn không thì sẽ phải tiếp tục làm và sửa chữa\n\n- business rules analysis – phát triển trong từng sprint đi kèm từng ui prototype thì sẽ có 1 bộ business rules đi kèm\n\n- data flow diagram, sequence dagram – để làm rõ hơn về quá trình cũng như luồng chạy của dự liều, trong các file ui thường sẽ cố gắng có ít nhất 1 trong 2 loại diagram trên.\n\n- prototyping\n\n- user stories – đây là kỹ thuật chỉ áp dụng ở dự án vendor portal, vì phía bên mss không áp dụng phát triển theo user stories', 4, true, '', 2525, now(), now(), false),
        (2526, 'Trong quá trình phát triển và phân tích requirement cho các tính năng của dự án vendor portal và mss, tôi đều cố gắng tìm hiểu các hệ thống khác đang chạy như thế nào ( nhất là từ những hệ thống lớn và thông dụng), để đảm bảo tính năng mình đưa ra sắp tới sẽ thân thiện với người dùng, hoặc ít nhất là không quá khó hiểu với người dùng vì sự đối lập về thiết kế và giao diện đối với những trải nghiệm sẵn có của người dùng.', 3, true, '', 2526, now(), now(), false),
        (2527, 'Xác định được interface requirement của hệ thống vendor portal, đảm bảo không bị xung đột với cảm nhận và trải nghiệm của người dùng. \n\nbên cạnh đó, ngoài những yêu cầu về functions và features, những yêu cầu về non-functional của dự án như là về security, performance, availability, user interface, safety... hiện tại mặc dù chưa tiến hành thực hiện nhưng dự án vp đang trong quá trình tìm hiểu và chuẩn bị cho migration và supportability', 3, true, '', 2527, now(), now(), false),
        (2528, 'Acceptance and evaluation criteria\nbusiness capability analysis\nbusiness rules analysis\ndata flow diagrams\ndata modeling\nglossary\ninterface analysis\nnon-functional requirements analysis\nprocess modeling\nprototyping\nroles and permissions matrix\nsequence diagrams\nuse cases and scenarios\nuser stories', 3, true, '', 2528, now(), now(), false),
        (2529, 'Trong quá trình phân tích, hỗ trợ qc và dev bằng việc tìm hiểu thiết kế cần có, hoặc kỹ thuật có thể apply được để có thể phát triển sản phẩm. ví dụ như wi liên quan đến encrypt và decrypt của patient name của vendor portal, tìm hiểu những cách có thể để có thể discuss với devs để tìm ra những cách thuận tiện hơn.\n\ntrong requirement documents, cố gắng list ra được happy case và những exception case để hỗ trợ qc trong việc design tc và viết tcs', 3, true, '', 2529, now(), now(), false),
        (2530, '- đã áp dụng và sử dụng các loại diagram như sau:\n+ activity diagram\n+ sequence diagram\n+ use case diagram\n\n- tuy nhiên, xét về việc thực hiện srs tốt thì chưa nên slot này đã có thực hiện nhưng chưa tốt và cần cải thiện hơn.', 2, true, '', 2530, now(), now(), false),
        (2531, 'Đã và đang apply các technique nêu trên, tuy nhiên, nhứng technique đang sử dụng không còn mới lạ cho dự án, còn những technique mới thì chưa có cơ hội cũng như chưa thực sự hiểu rõ để có thể apply vào luôn đc.', 2, true, '', 2531, now(), now(), false),
        (2532, '- validation is the process of confirming the completeness and correctness of requirements\n\n- verification is the process of confirming that the designed and built product fully addresses documented requirements', 3, true, '', 2532, now(), now(), false),
        (2533, '- review requirement được chuẩn bị từ haptt\n- review requirement do bản thân chuẩn bị cùng po', 3, true, '', 2533, now(), now(), false),
        (2534, 'Khi tính năng được hoàn thành, tham gia vào việc verify requirement:\n\n- chuẩn bị 1 check-list trước khi verify. check list này thường được chuẩn bị dựa trên acceptance criteria và quá trình review tc của qc. từ đó list ra được happy cần cần phải thỏa mãn là gì, và 1 số exception cases.\n\n- trong quá trình verify, nếu phát hiện bugs sẽ list lại rồi review và confirm với qc để tránh việc log bug bị duplicate, và để phía qc chú ý hơn trong quá trình test', 3, true, '', 2534, now(), now(), false),
        (2535, 'Trong quá trình review requirement cho đồng nghiệp, thường sẽ cố gắng để review kỹ để chỉ ra lỗi cho các bạn sửa và cải thiện chất lượng requirement của mình.\n\n- review về lỗi logic \n\n- review về lỗi chính tả, câu cú', 4, true, '', 2535, now(), now(), false),
        (2536, 'Để lại comment bao gồm:\n\n- lỗi gặp phải trong từng cell\n\n- đề xuất sửa ( chính tả, câu cú, hoặc logic)\n\n240302020 – trong quá trình làm việc cho dự án scs, ngoài việc thực hiện review thì còn đề xuất hướng giải quyết cho các ba khác để có thể không chỉ đạt được yêu cầu như kh mong mà còn có thể đề xuất được những requirement khác sao cho sản phẩm và flow chạy được tốt nhất.', 4, true, '', 2536, now(), now(), false),
        (2537, 'Meeting với khách hàng trong meeting của vendor portal hoặc meeting cá nhân của ba team với michele để trao đổi về requirement, review cũng như lấy feedback\n\n24032020 – hiện tại ở dự án scs đang là người thực hiện việc take lead và take note cho các buổi requirement discussion, sau đó về distribute cũng như follow up với ba khác để đảm bảo requirement được hiểu đúng và đủ.', 4, true, '', 2537, now(), now(), false),
        (2538, 'Prototype được chuẩn bị từ giai đoạn đầu khi start wi và được cập nhật tùy theo feedback của khách hàng để đảm bảo chất lượng requirement', 3, true, '', 2538, now(), now(), false),
        (2539, 'Đối với từng bước thì áp dụng các kỹ thuật, các technique khác nhau để đảm bảo sẽ đưa ra được đúng và đủ.\n\n- đối với việc phân tích\n+ ui prototyping\n+ use cases diagram\n+ sequence diagram\n+ data flow\n\n- đối với việc verify requirement\n+ acceptance & evaluation criteria\n+ item tracking / checklists\n+ reviews', 3, true, '', 2539, now(), now(), false),
        (2540, 'Slot 3c ở trên e chưa có commit nên với mô tả của slot này là cần pass slot 3c nên em không đánh giá slot này ạ.', 1, true, '', 2540, now(), now(), false),
