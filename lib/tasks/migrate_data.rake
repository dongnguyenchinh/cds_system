      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(166, 'Have knowledge of basic requirement elicitation techniques', 'Có kiến thức cơ bản về các elicitation techniques hoặc đã được training về Requirement Elicitation Techniques', 1, 22, 166);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(167, 'Assist in engaging different stakeholders to determine needs and requirements', 'Ít nhất 3 bằng chứng hỗ trợ làm việc với các stakeholders trong quá trình chuẩn bị elicit (collect documents, prepare surveys, questions list, sắp xếp lịch hẹn…), làm plan (slot 2a), MoM sau khi elicit', 1, 22, 167);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(168, 'Assist in applying different techniques to the project as appropriate to elicit requirements', 'Ít nhất 3 bằng chứng hỗ trợ người khác thực hiện elicitation thông qua việc áp dụng 10 kỹ thuật được mô tả tại slot 2b', 1, 22, 168);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(169, 'Assist in creating requirement elicitation plan', 'Thường xuyên được yêu cầu từ manager để hỗ trợ công tác hoạch định cho quá trình elicit requirements', 1, 22, 169);
      ---- level 1 -- end
      ---- level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(170, 'Develop requirement elicitation plan, agendas', '<p>Mỗi 1 Elicitation session cần được chuẩn bị đầy đủ các bước bao gồm:<br />+ Clarify the scope for each elicitation session, select technique, gather supporting materials<br />+ Scheduled resources (stakeholders, facilities, equipment)<br />+ Notify the plan (invitation, distribution), được gửi bởi PIC của buổi Elicitation, không nhất thiết phải là người làm plan<br />Nếu thiếu 1 phần nào ở trên thì việc chuẩn bị coi như chưa hoàn tất / chưa đạt yêu cầu. Do đó, 1 Elicitation Plan đạt yêu cầu là 1 bản kế hoạch cover những phần trên cho tối thiểu 3 chức năng trong ứng dụng</p>', 2, 22, 170);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(171, 'Apply at least 1 technique in minimum 6 months to engage different stakeholders to determine needs and requirements', '<p>Dùng 1/10 techniques bên dưới thành thạo để elicit requirements ít nhất 6 tháng<br />+ Event: Brainstorming, focus groups, interviews, observation, prototyping, requirements workshops<br />+ Document Analysis, interface analysis<br />+ Distributed: Survey / questionnaire<br />Bằng chứng phải thể hiện được kết quả Elicitation được confirmed bởi khách hàng / Direct Manager / các stakeholders khác có liên quan</p>', 2, 22, 171);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(172, 'Apply 3/10 different elicitation techniques to the project as appropriate to elicit requirements in minimum 6 months', 'Có bằng chứng dùng thành thạo 3/10 techniques trên trong ít nhất 6 tháng', 2, 22, 172);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(173, 'Able to identify and analyze conflicts between stakeholders in requirements elicitation (identification of conflict, analysis of sources, participation in resolution)', '<p>Nhận diện conflict trong requirements của các stakeholders, raise lên kịp thời, phân tích và tham gia vào quá trình tìm giải pháp để resolve cái conflict<br />+ Conflict có thể về priority về details của requirements hoặc giữa các requirements được các stakeholders khác nhau mô tả<br />+ Xác định nguồn của conflict: just data, or conflict of interest, conflict of value, relationship conflict, structural conflict</p>', 2, 22, 173);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(174, 'Apply appropriate methods to engage and communicate with stakeholders in requirements activities. On the other hands, able to plan and deliver right messages in right formats at right time, to right stakeholders', '<p>Có bằng chứng của việc áp dụng thành công các hình thức để engage và communicate requirements: đúng hình thức, đúng thông tin và thời điểm, tới đúng đối tượng, không có phản ví dụ<br />+ Xác định rõ khi nào cần formal: documents, emails, presentations, workshops để communicate với stakeholders; khi nào informal: chats, call, f2f, group discussions<br />+ Loại packaging requirements là: text, model, mockup, interactive prototype, live demo...</p>', 1, 22, 174);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(175, 'Analyze and negotiate conflicts between stakeholders in requirements elicitation by applying conflict resolution related techniques', '<p>Slot này là 1 level cao hơn 2d ở trên. Sau khi xác định và phân tích conflicts, cần có bằng chứng về việc đề xuất giải pháp để resolve conflict. Giải pháp đề xuất có thể được team chấp nhận hoặc reject.<br />Để pass được slot này, cần có bằng chứng cho việc phân tích pros / cons của các alternatives và từ đó dùng các kỹ thuật như vote, consensus, decision matrix,... để negotiate với các stakeholders liên quan và thống nhất giải pháp cho conflict</p>', 1, 22, 175);
      ---- level 3 -- end
      ---- level 4 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(176, 'Recommend and apply ways to engage and communicate with stakeholders, the management team, and developers in requirements activities that are new to project / department', 'Có bằng chứng đề xuất và áp dụng hiệu quả cách thức mới để engage, communicate với các stakeholders mà trước đó trong dự án, department chưa dùng. Có seminar / meeting chia sẻ cách áp dụng với sự tham gia của PM / direct manager', 1, 22, 176);
      ---- level 4 -- end
      -- competency Requirement Elicitation  -- end
      -- competency Requirement Analysis -- start
      ---- level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(177, 'Assist in domain / requirement research and analysis', 'Ít nhất 3 bằng chứng hỗ trợ research, phân tích về 1 domain hay requirement nào đó', 1, 23, 177);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(178, 'Able to perform research and analysis tasks about domain related information, data, materials under supervision of direct managers', 'Thực hiện task research / analysis với output, cách làm được nêu rõ bởi manager và được đánh giá là đạt yêu cầu. không tự đề xuất WBS, cách làm', 1, 23, 178);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(179, 'Having knowledge of common requirement analysis techniques / methods', '<p>Có kiến thức cơ bản về các analysis techniques hoặc đã được training về chúng:<br />Research / document analysis<br />Prioritization techniques: Decision analysis, risk analysis, MoSCoW, Voting, Timeboxing/ Budgeting<br />Assumptions &amp; Constraints<br />Requirements Organization / Allocation: a subset of requirement specification techniques</p>', 1, 23, 179);
      ---- level 1 -- end
      ---- level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(180, 'Assist to prioritize requirements', 'Ít nhất 3 bằng chứng hỗ trợ trong việc sử dụng các Prioritization Techniques đề cập ở slot 1c để prioritize requirements', 2, 23, 180);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(181, 'Assist to allocate and derive requirements', 'Ít nhất 3 bằng chứng hỗ trợ trong việc sử dụng các Requirements Organization / Allocation techniques đề cập ở slot 1c để allocate và derive requirements', 2, 23, 181);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(182, 'Apply appropriate requirement analysis methods under supervision of direct managers', 'Áp dụng kỹ thuật phân tích phù hợp đề cập ở slot 1c để cho ra output / đề xuất, có sự hỗ trợ của direct managers', 3, 23, 182);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(183, 'Prioritize requirements by applying at least 1 prioritization technique in minimum 6 months and there are no significant changes on the resulted priorities of important requirements', 'Đề xuất độ ưu tiên bằng cách áp dụng ít nhất 1 kỹ thuật prioritization bên trên, với ít phản biện / thay đổi; không có phản biện / thay đổi đáng kể về độ ưu tiên của 1 high priority function / feature', 3, 23, 183);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(184, 'Able to develop high quality Feature List or Use case model or similar requirement spec for at least 2-3 small-to-medium projects (e.g. size of 20+ man-months) or 1 big project (e.g. size of 50+ man-months)', '<p>Đề xuất Features List / Use case phù hợp bằng cách áp dụng các kỹ thuật:<br />Data Modeling<br />Functional Decomposition<br />Interviews<br />Organizational Modeling<br />Scope Modeling<br />Workshops</p>', 3, 23, 184);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(185, 'Able to provide suggestions that brings more values to the solution throughout the software development life cycle such as new idea, non-functional requirements, architecture, user experience', 'Đưa ra các đề xuất để tăng value cho solution dựa trên cơ sở phân tích / lập luận logic (nhiều alternatives, phân tích pros / cons…). Tăng value / emergent solution: 1+1>2; thông qua non-functional requirements, kiến trúc, UI / UX…', 3, 23, 185);
      ---- level 3 -- end
      -- competency Requirement Analysis  -- end
      -- competency Requirement Specification -- start
      ---- level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(186, 'Have basic knowledge of techniques and tools used for requirement specification and modeling', '<p>23 SMR (Specify and Model Requirements) Techniques:<br />Acceptance and Evaluation Criteria<br />Business Capability Analysis<br />Business Model Canvas<br />Business Rules Analysis<br />Concept Modeling<br />Data Dictionary<br />Data Flow Diagrams<br />Data Modeling<br />Functional Decomposition<br />Glossary<br />Interface Analysis<br />Non-Functional Requirements Analysis<br />Organizational Modeling<br />Process Modeling<br />Prototyping<br />Roles and Permissions Matrix<br />Root Cause Analysis<br />Scope Modeling<br />Sequence Diagrams<br />Stakeholder List, Map, or Personas<br />State Modeling<br />Use Cases and Scenarios<br />User Stories<br />Có kiến thức cơ bản về các SMR techniques trên hoặc đã được training về chúng</p>', 1, 24, 186);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(187, 'Assist with preparation of requirements for consistency with internal and published standards', 'Không có phản ví dụ về tính consistency của requirements (UI, wording trong specs / terms, workflow…) trong nội bộ dự án và với bên ngoài (các ứng dụng well-known)', 1, 24, 187);
      ---- level 1 -- end
      ---- level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(188, 'Able to identify and specify interfaces and non-functional requirements in addition to functional requirements for at least 2-3 small-to-medium projects (e.g. size of 20+ man-months) or 1 big project (e.g. size of 50+ man-months)', 'Có bằng chứng xác định interface requirements và non-functional requirements trong 2-3 small to medium projects / 1 big project. không có phản ví dụ missing interface hoặc non-functional requirements quan trọng / dễ nhìn thấy tương ứng với 1 hoặc vài functions', 2, 24, 188);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(189, 'At least 1 year experience in specifying and modeling key requirement outputs for big software project (e.g. with team size of 10+ or project size of 50+ man-months) such as business rules, message logs, use cases, workflow diagram, UI prototypes... using at least 12/23 SMR techniques', 'Lựa chọn và áp dụng thành công tối thiểu 12/23 techniques nêu trên (đúng giai đoạn, đúng thông tin, đúng techniques, đúng đối tượng) trong các dự án lớn', 2, 24, 189);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(190, 'Recognized by other teams (development, testing) in delivering good outputs for further use in development and testing', '<p>Trong quá trình analysis và specification có tạo ra các outputs hỗ trợ cho việc distribute / reuse để developer / qc có thể dùng trong công việc của họ -&gt; time savings<br />Requirements specs go beyond yêu cầu cần có của specs đó, có thêm thông tin hỗ trợ cho các giai đoạn phát triển dự án / quản lý công việc</p>', 2, 24, 190);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(191, 'Lead development of the SRS using at least 3 types of UML diagrams to specify and model requirements', '<p>Phát thảo skeleton for SRS trong dự án cụ thể, phân chia việc phát triển nội dung giữa các BAs, tiến hành, giám sát cho đến khi đạt kết quả<br />Trong SRS dùng tối thiểu 3 loại UML diagrams để mô tả requirements</p>', 3, 24, 191);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(192, 'Recommend and apply requirements specification methods that are new to project / department', 'Apply thành công tối thiểu 2/23 techniques nêu trên với điều kiện các techniques này trước đó dự án chưa dùng => bạn này phải dùng thành thạo 14/23 techniques (passed slot 2b + 2 more). Có seminar chia sẻ cách làm với các thành viên khác trong dự án / phòng ban', 3, 24, 192);
      ---- level 3 -- end
      -- competency Requirement Specification  -- end
      -- competency Requirement Verification & Validation -- start
      ---- level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(193, 'Able to differentiate between verification and validation by real example then explain the purposes and basic processes of requirement verification and validation', 'Phân biệt được VER và VAL nói chung cũng như VER và VAL trong requirement processes: trả lời được bước nào trong R&R process là VER, bước nào là VAL, mục đích tương ứng', 1, 25, 193);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(194, 'Able to assist in verifying requirement specification outputs with guidance from leaders', 'Hỗ trợ quá trình review requirements theo best practices / principles. Tham gia review dưới sự hướng dẫn của leader với các findings.', 1, 25, 194);
      ---- level 1 -- end
      ---- level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(195, 'At least 1 year experience in using common verification and validation techniques / tools', '<p>Bằng chứng áp dụng các kỹ thuật để verify requirements tối thiểu 1 năm:<br />+ Acceptance &amp; Evaluation Criteria<br />+ Item tracking / Checklists<br />+ Quality metrics and KPIs<br />+ Reviews</p>', 2, 25, 195);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(196, 'Proactively review requirements specifications for errors and omissions. Regularly raise requirements defects through either offline or online reviews in a constructive manner', 'Offline / online Review req của người khác và thường xuyên phát hiện các lỗi trong requirements, đóng góp tích cực', 2, 25, 196);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(197, 'Able to provide fixes to defects found during requirement specification reviews', 'Thường xuyên / tích cực đề xuất giải pháp để sửa các lỗi requirements phát hiện được trong quá trình review', 2, 25, 197);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(198, 'Lead at least 3 requirements specification reviews with attendances of different roles and higher management and / or client', 'Lead ít nhất 3 buổi requirements review thành công (trong đó 1 buổi với khách hàng / higher management)', 3, 25, 198);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(199, 'Create different types of prototypes for requirements validation at different stages of requirement life cycle', '<p>Có bằng chứng tạo prototypes để validate requirements ở các giai đoạn khác nhau của dự án:<br />+ Process modeling đ����� capture current state / đề xuất future state<br />+ Benchmarking / Quick demo những site / ứng dụng tương tự với nhu cầu của khách hàng<br />+ Create prototype: mockup → interactive wireframes</p>', 3, 25, 199);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(200, 'Able to identify and analyze conflicts between stakeholders in requirements verification (identification of conflict, analysis of sources, participation in resolution)', '<p>Xác định nguồn của conflict: just data, or conflict of interest, conflict of value, relationship conflict, structural conflict, từ đó define resolution<br />Bằng chứng tương tự như slot 3b Elicitation nhưng khó hơn vì áp dụng cho conflict liên quan đến độ ưu tiên, usability của tính năng, UX, business values...</p>', 3, 25, 200);
      ---- level 3 -- end
      ---- level 4 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(201, 'Apply the most appropriate formal and informal requirements validation and verification techniques', '<p>+ Document Analysis<br />+ Financial Analysis<br />+ Item tracking / Checklists<br />+ Quality metrics and KPIs<br />+ Reviews<br />+ Risk Analysis and Management (business objectives, current state → future state, potential value)</p><p>Bằng chứng lựa chọn và áp dụng đúng các kỹ thuật để verify requirements trong các tình huống khác nhau<br />+ Acceptance &amp; Evaluation Criteria<br />+ Item tracking / Checklists<br />+ Quality metrics and KPIs<br />+ Reviews</p><p>Bằng chứng lựa chọn và áp dụng đúng các kỹ thuật để validate requirements trong các tình huống khác nhau<br />+ Acceptance &amp; Evaluation Criteria<br />+ Document Analysis<br />+ Financial Analysis<br />+ Item tracking / Checklists<br />+ Quality metrics and KPIs<br />+ Reviews<br />+ Risk Analysis and Management (business objectives, current state → future state, potential value)</p>', 4, 25, 201);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(202, 'Analyze and negotiate conflicts between stakeholders in requirements verification by applying conflict resolution related techniques', '<p>Define solution từ việc phân tích conflicts -&gt; Phải passed được slot 3c của Req Verification &amp; Validation ở trên<br />Thêm vào đó cần, negotiate với các stakeholders để resolve conflicts bằng cách áp dụng các kỹ thuật vote, consensus, decision matrix,… để giải quyết các conflicts liên quan đến độ ưu tiên, usability của tính năng, UX, business values...</p>', 4, 25, 202);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(203, 'Assess solution performance and recommend actions to eliminate limitations / increase value', '<p>Dựa trên các Quality metrics, KPIs, Acceptance Criteria ban đầu đặt ra → so sánh với prototype / solution được defined → xác định các điểm pros &amp; cons, các limitations và đề xuất các actions để tăng value của solution<br />Điều này có thể làm song song khi đề xuất các options của prototype hoặc là trong quá trình UAT solution -&gt; bằng chứng có thể tìm thấy trong các giai đoạn đó</p>', 4, 25, 203);
      ---- level 4 -- end
      -- competency Requirement Verification & Validation  -- end
      -- competency Requirement Monitoring & Controlling -- start
      ---- level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(204, 'Assist in maintaining requirement traceability', '<p>Cập nhật RTM và các tools khác trong dự án để đảm bảo requirements được trace 2 chiều: giữa các requirements và giữa requirements với development status.<br />Khi được hỏi thì mất không quá 5 phút để biết mối quan hệ giữa các req và development status của các requirements. RTM phải được up-to-date</p>', 1, 26, 204);
      ---- level 1 -- end
      ---- level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(205, 'Assist in verifying change requests and evaluating their impacts to the baselined requirements', 'Có bằng chứng dùng RTM hoặc cách nào đó để xác định / phân tích impacts của CR, làm input cho 3a bên dưới', 2, 26, 205);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(206, 'Verify change requests and evaluate their impacts to the baselined requirements', 'Dùng RTM và / hoặc các tools / techniques khác hiệu quả để xác định / phân tích impacts của CR đến requirements và các giai đoạn trong quá trình phát triển phần mềm', 3, 26, 206);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(207, 'Able to identify and analyze conflicts between stakeholders / baselined requirements in change request verification and impact evaluation (identification of conflict, analysis of sources, participation in resolution)', '<p>Xác định nguồn của conflict: just data, or conflict of interest, conflict of value, relationship conflict, structural conflict, từ đó define resolution</p><p>Review với các stakeholders khác để đồng thuận solution cho conflict liên quan đến bản thân CR hoặc impacts analysis</p>', 3, 26, 207);
      ---- level 3 -- end
      ---- level 4 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(208, 'Lead the development of change request implementation plan', 'Lập kế hoạch hiện thực CR dựa trên kết quả từ 3a & 3b', 4, 26, 208);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(209, 'Lead change request development', 'Chịu trách nhiệm chính trong việc quản lý quá trình hiện thực CR theo plan từ 4a', 4, 26, 209);
      ---- level 4 -- end
      ---- level 5 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(210, 'Apply requirements monitoring and controlling methods that are new to project / department', 'Đề xuất và áp dụng hiệu quả cách thức mới quản lý requirements mà trước đó trong dự án, department chưa dùng', 5, 26, 210);
      ---- level 5 -- end
      -- competency Requirement Monitoring & Controlling -- end
    SQL

    # execute query
    Template.connection.execute(sql_template)
