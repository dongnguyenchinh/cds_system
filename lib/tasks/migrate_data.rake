      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(546, 'Identify problems, think through potential solutions then communicate and/or escalate appropriately.', '<p><em><span style="text-decoration: underline;"><strong>Phản ví dụ:</strong></span> Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p>', 1, 45, 12);
