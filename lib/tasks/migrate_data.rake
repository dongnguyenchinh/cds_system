  desc "insert slots for developers"
  task :migrate_data_slots_dev do
    sql_template = <<-SQL
      insert into templates(id, name, description, role_id, user_id, created_at, updated_at) values
        (1, 'CDS/CDP for Developers', "Template Career Development Plan / Career Development System For Developers", 1, 1, now(), now());
    SQL

    sql_competency = <<-SQL
      insert into competencies(id, name, `desc`, _type, template_id, location, created_at, updated_at) values
        (1, 'Productivity', 'Productivity', 'General', 1, 1, now(), now()),
        (2, 'Problem Solving', 'Problem Solving', 'General', 1, 2, now(), now()),
        (3, 'Communication', 'Communication', 'General', 1, 3, now(), now()),
        (4, 'Planning & Organizing', 'Planning & Organizing', 'General', 1, 4, now(), now()),
        (5, 'Computer Science', 'Computer Science', 'Specialized', 1, 5, now(), now()),
        (6, 'Software Engineering', 'Software Engineering', 'Specialized', 1, 6, now(), now()),
        (7, 'Programming', 'Programming', 'Specialized', 1, 7, now(), now());
    SQL

    sql_slot = <<-SQL
      insert into slots(id, `desc`, evidence, level, competency_id, slot_id, created_at, updated_at) values
        (1, 'Consistently perform well under supervision', '<p>Nếu được giao việc và được hướng dẫn cách làm thì hoàn tất tốt công việc được giao trong mức độ năng suất hợp lý<br />Nếu có sai sót trong quá trình làm việc và đã được nhắc nhở về những lỗi cụ thể thì không lặp lại những lỗi này.</p>', 1, 1, 1, now(), now()),
        (2, 'Recognized as a contributing member of the team.', "<p>Là một thành viên đóng góp tích cực cho nhóm / dự án. Những người khác không thấy nhân viên này là điểm yếu, là gánh nặng cho team trong productivity</p\> <p><span style='text-decoration: underline;'><strong>Phản ví dụ:</strong></span> <em>Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p>", 1, 1, 2, now(), now()),
        (3, 'Complete assigned tasks within scheduled completion dates. Communicate potential issues as soon as you are known.', '<p>Hoàn tất công việc được giao một cách đúng hạn, với chất lượng tốt. Trao đổi, thông báo ngay về những vấn đề tiềm năng khi phát hiện ra chúng</p>', 1, 1, 3, now(), now()),
        (4, 'Demonstrate a high level understanding of and compile with Company standards, processes and procedures.', '<p>Hiểu rõ và tuân thủ tốt các quy trình, quy định, thủ tục, chuẩn do Công ty đang áp dụng</p>', 1, 1, 4, now(), now()),
        (5, 'Capable of locating and effectively using detailed information from the Company Portal, Mantis, SVN, HRM, …', '', 1, 1, 5, now(), now()),
        (6, 'Recognized as a proactive member of the team.', "<p>Là một thành viên đóng góp tích cực cho nhóm / dự án. Nằm trong số những người đóng góp hàng đầu cho dự án.</p><p><span style='text-decoration: underline;'><strong>Phản ví dụ:</strong></span> <em>Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p>", 2, 1, 6, now(), now()),
        (7, 'Complete critical tasks on time', "<p><strong>Critical tasks:</strong> Những công việc quan trọng, chủ chốt, cốt lõi trong dự án, trong bộ phận mà nhân viên tham gia, ở góc độ vai trò tương ứng.</p><p><span style='text-decoration: underline;'><strong>Ví dụ:</strong> </span><br /><em>Nhân viên làm Business Analyst trong một dự án thì critical tasks là những công việc chủ chốt, cốt lõi liên quan đến phân tích yêu cầu khách hàng</em></p><p><em>Nhân viên làm Developer trong một dự án thì critical tasks là những công việc liên quan đến thiết kế / tài liệu hóa / lập trình / sửa bugs của những thứ quan trọng, chủ chốt, cốt lõi.</em></p><p><em>Nhân viên làm Tester trong một dự án thì thì critical tasks là những công việc liên quan đến việc phát triển test plan / test cases / tiến hành test execution của những thứ quan trọng, chủ chốt, cốt lõi trong dự án.</em></p>", 2, 1, 7, now(), now()),
        (8, 'Able to represent the skills and capabilities of your department / practice unit / business unit. ', '<p><strong>Represent:</strong> Đại diện.</p><p>Có nhiều mức độ đại diện – <strong>ví dụ</strong></p><p><em>Đại diện cho dự án để training cho một nhân viên mới, training cho sinh viên thực tập về kiến thức, kỹ năng ở vai trò của mình;</em></p><p><em>Đại diện cho dự án để làm việc với dự án khác; đại diện cho dự án làm việc với khách hàng;</em></p><p>Có khả năng chịu trách nhiệm chính trong dự án về chuyên môn mà mình đang đảm nhận;</p><p>Đại diện cho bộ phận chuyên môn của mình ở phạm vi công ty để làm việc với bất cứ đối tượng nào khi cần</p><p>Nhân viên sẽ được đánh giá pass slot này nếu có bằng chứng thuyết phục liên quan đến khả năng chịu trách nhiệm chính về chuyên môn trong dự án hiện tại và có khả năng đại diện cho bộ phận chuyên môn của mình ở phạm vi công ty để làm việc với bất cứ đối tượng nào khi cần</p>', 2, 1, 8, now(), now()),
        (9, 'Recognized internally as a solid knowledge resource.', '<p>Cứ có vấn đề gì về mảng kiến thức mà cần trợ giúp / câu trả lời là nghĩ ngay đến nhân viên này</p>', 3, 1, 9, now(), now()),
        (10, 'Successfully complete tasks and assignments independently and supervise the work of others as requested.', '<p>Nhân viên sẽ được đánh giá pass slot này nếu có bằng chứng thuyết phục:<br />- Khi được giao việc thì hoàn tất được công việc một cách độc lập, không cần có sự chỉ dẫn về cách làm, và<br />- Có thể hướng dẫn, hỗ trợ, giám sát người khác [vài người] hoàn tất công việc</p>', 3, 1, 10, now(), now()),
        (11, 'Regularly requested by Company or Business Unit leadership and clients for advice and guidance.', '<p>Có khả năng tư vấn, định hướng, dẫn đường cho công ty hoặc khách hàng về: <br />- Chuyên môn kỹ thuật ở vai trò mình nắm giữ, hoặc <br />- Trong những vấn đề liên quan đến hoạt động chung của công ty</p>', 3, 1, 11, now(), now()),
        (12, 'Identify problems, think through potential solutions then communicate and/or escalate appropriately.', '<p><em><strong>Phản ví dụ:</strong></em> <em>Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p>', 1, 2, 12, now(), now()),
        (13, 'Identify and solve simple problems independently (for example: recognizing changes in scope and communicating them).', '<p>Không cần hỏi người khác về giải pháp cho vấn đề [đơn giản] đang gặp phải</p>', 2, 2, 13, now(), now()),
        (14, 'Provide assistance in solving complex problems (complex – technically difficult, opposing viewpoints, risky, and/or sensitive).', 'Đề xuất, hỗ trợ được trong việc giải quyết các vấn đề phức tạp', 2, 2, 14, now(), now()),
        (15, 'Lead simple meetings for internal or external clients. Peer and/or next level management are likely to attend these meetings.', 'Bằng chứng thuyết phục là có dính dáng đến khách hàng bên ngoài', 2, 2, 15, now(), now()),
        (16, 'Possess and use good diagnosis / troubleshooting skills.', '', 2, 2, 16, now(), now()),
        (17, 'Identifies and solves complex or sensitive problems (for example: associate performance, project scope issues, and priority changes or resolving multiple conflicting agendas). ', '<p>- Không cần hỏi người khác về giải pháp cho vấn đề phức tạp đang gặp phải.<br />- Thế nào là vấn đề phức tạp hoặc nhạy cảm trong phát triển phần mềm?<br />- Yêu cầu của khách hàng phức tạp, logic xử lý phức tạp, giải thuật phức tạp và có đòi hỏi cao về performance, security; đòi hỏi hiểu biết cao để ứng dụng các frameworks sẵn có thay vì phải hiện thực từ đầu; phải xử lý các vấn đề phức tạp đồng thời</p>', 3, 2, 17, now(), now()),
        (18, 'Breaks down major issues and problems into workable pieces; sees whole situation behind a problem or issue; understands the causes, effects, implications; sets priorities and takes right action for resolution.', '<p>- Có khả năng chia nhỏ vấn đề lớn, phức tạp thành những phần nhỏ hơn để xử lý;</p><p>- Nắm được bức tranh tổng thể của một vấn đề phức tạp; hiểu rõ nguyên nhân, hậu quả, những vấn đề có liên quan; thiết lập được độ ưu tiên, thứ tự và tiến hành các công việc phù hợp để giải quyết bài toán</p>', 3, 2, 18, now(), now()),
        (19, 'Identify and solve complex or sensitive problems at the strategic executive level.', "<p><span style='text-decoration: underline;'><strong>Ví dụ:</strong></span></p><p><em>Phát biểu bài toán 7 năm của công ty và giải quyết nó</em></p><p><em>Phát biểu bài toán technical lead của công ty và giải quyết nó</em></p><p><em>Phát biểu bài toán quản lý chi phí / năng suất của công ty và giải quyết nó</em></p>", 4, 2, 19, now(), now()),
        (20, 'Anticipate project and / or assignment risks and provide potential solutions.', '', 4, 2, 20, now(), now()),
        (21, 'Capable of internal interaction. This is evident in all core communications.', "<p><em><span style='text-decoration: underline;'><strong>Example:</strong> </span></em><br /><em>Writing: Weekly report, email, Bug tracker, update document, etc.</em><br /><em>Reading and understanding: Project documents, Company policy, Guideline (Bug tracker, SVN, HRM), etc. </em><br /><em>Communicating with team member: discuss and raise issues as soon as they are known</em></p>", 1, 3, 21, now(), now()),
        (22, 'Effectively attend simple meetings', '', 1, 3, 22, now(), now()),
        (23, 'Lead simple internal meetings. Peers and/or next level management are likely to attend these meetings', '', 2, 3, 23, now(), now()),
        (24, 'Capable of client interaction with supervision or guidance from management or team leadership', '', 2, 3, 24, now(), now()),
        (25, 'Exhibit developed good written and verbal communications. This is evident in complete, accurate and timely status reports and project deliverables as well as clear and concise direct communication.', '<p><strong>Good:</strong> Có khả năng tốt về written &amp; verbal communications, ít khi phải hỏi ai về written &amp; verbal communications. Có thể chịu trách nhiệm chính về written &amp; verbal communications cho 1 hay nhiều dự án [giới hạn ở vai trò của mình].</p>', 2, 3, 25, now(), now()),
        (26, 'Demonstrated ability to transfer knowledge to junior associates and thus serve as a mentor.', 'Cho sinh viên thực tập hay cho nhân viên mới? Bao nhiêu người? Level mentoring cỡ nào?', 2, 3, 26, now(), now()),
        (27, 'Effectively deliver unpopular and / or difficult messages with an understanding of the receiver’s viewpoint.', '<p>Một key member [Senior Software Engineer hoặc tương ứng trở lên] muốn rời khỏi công ty, thậm chí đã gửi đơn xin nghỉ việc. Thuyết phục người này ở lại bằng cách nào?</p><p>Khách hàng không có ngân sách thuê ngoài dịch vụ training về quản lý dự án trong khi Công ty lại .... làm sao để thuyết phục khách hàng?</p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty. Ví dụ cụ thể: Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</p>', 3, 3, 27, now(), now()),
        (28, 'Demonstrate strong written and verbal communication skills. (Example: formal documents, client/peer or conference presentations).', "<p><strong>Strong:</strong> written &amp; verbal communications là sở trường, lợi thế, sở thích. Được khách hàng, đồng nghiệp, cấp trên khen ngợi, bái phục. Được mọi người tham khảo ý kiến khi cần communicate cho tốt hơn</p><p><span style='text-decoration: underline;'><strong>Phản ví dụ:</strong></span> <em> Khoảng 4:00 PM thấy ứng dụng cho khách hàng [bản production] deploy tại www.abc.com bị down và không giải quyết được. 5:30 PM cứ thế đi về nhà mà không báo cho cấp trên để giải quyết triệt để</em></p><p>Tìm hiểu được thông tin từ khách hàng xem mức chi phí mà khách hàng chấp nhận cho 1 dự án fixed cost là bao nhiêu, hourly rate khách hàng chấp nhận được là bao nhiêu, có xét tới lợi ích của công ty.</p><p><span style='text-decoration: underline;'><strong>Ví dụ cụ thể:</strong></span><em> Ước lượng chúng ta cần charge khách hàng 530 working hours thì mới có mức lãi như kỳ vọng, nhưng chúng ta không biết mức độ chấp nhận của khách hàng thế nào, cần tìm hiểu thông tin từ khách hàng trước khi gửi bản đề xuất chính thức</em></p>", 3, 3, 28, now(), now()),
        (29, 'Lead or facilitate complex meetings for either internal or external clients. The next level of management likely attends these meetings. (Example: formal reviews, workshops, requirements sessions).', '', 3, 3, 29, now(), now()),
        (30, 'Display good negotiating skills. (Example: setting scope and time-lines with client, vendor discussions, and technical strategies.)', "<p><span style='text-decoration: underline;'><strong><em>Ví dụ 1 về good negotiating skills:</em></strong></span></p><p><em>Khách hàng nói: Dự án phải được hoàn tất trong phạm vi dưới 300 triệu đồng</em><br /><em>Khi dự toán thì thấy do phải mua một số components nên tổng dự toán của dự án là 340 triệu [280 + 60].</em></p><p><em>Nếu thương lượng được để khách hàng đồng ý trả chúng ta 280 triệu còn 60 triệu chi phí mua components khách hàng thanh toán riêng ==&gt; good negotiating skills</em></p><p><em><span style='text-decoration: underline;'><strong>Ví dụ 2 về good negotiating skills:</strong></span></em></p><p><em>Khách hàng muốn tiến hành dự án với 3 billable per month in 12 months. Tuy nhiên khách hàng lại muốn trả giá hourly rate thật là thấp, giả sử US$ 8.00.</em></p><p><em>Nếu thương lượng được để khách hàng đồng ý trả chúng ta hourly rate US$ 12.00 với cam kết rằng khối lượng công việc chúng ta hoàn tất được là xứng đáng với hourly rate US$ 12.00 ==&gt; good negotiating skills</em></p><p><span style='text-decoration: underline;'><strong><em>Ví dụ 3 về good negotiating skills:</em></strong></span></p><p><em>Một key member muốn rời khỏi công ty, thậm chí đã gửi đơn xin nghỉ việc. Thuyết phục người này ở lại bằng cách nào?</em></p><p><strong><span style='text-decoration: underline;'><em>Ví dụ 4 về good negotiating skills:</em></span></strong></p><p><em>Một key member đã rời công ty và đi làm ở công ty khác. Công ty cần người này làm part time vào các thứ bảy, chủ nhật, buổi tối trong một khoảng thời gian. Vậy khoảng thời gian này có tính là overtime với hệ số 1,5 hay không? ==&gt; thương lượng là nó chỉ hệ số 1</em></p>", 4, 3, 30, now(), now()),
        (31, 'Exhibit the appropriate interpersonal skills required to establish effective working relationships with clients and/or business partners', '<p>Thế nào là interpersonal skills? <a href="http://en.wikipedia.org/wiki/Interpersonal">http://en.wikipedia.org/wiki/Interpersonal</a> <br />Nói về Interpersonal Relationship, dựa vào đó sẽ cần có các skills để Flourishing Relationships<br />Hãy chứng minh bạn có Appropriate Interpersonal Skills!</p>', 4, 3, 31, now(), now()),
        (32, 'Demonstrate excellent written and verbal communication skills as well as facilitation skills.', '<p>Không chỉ strong, mà còn phải biết hát. Là một người cực kỳ xuất sắc về communications. Rất hiếm người đạt được slot này.</p><p>Nếu làm việc với khách hàng nước ngoài bằng tiếng Anh thì một điều kiện cần là TOEIC 900?</p>', 4, 3, 32, now(), now()),
        (33, 'Lead or facilitate complex, multi-session, politically charged meetings for clients. The most senior members of the client (internal or external) team are likely to attend these meetings representing their own group’s interest. ', '', 4, 3, 33, now(), now()),
        (34, 'Lead or facilitate complex, multi-session, politically charged meetings for clients. The most senior members of the client (internal or external) team are likely to attend these meetings representing their own group’s interest. ', '<p>Chịu trách nhiệm chính từ phía Công ty trong các buổi họp phức tạp, khó khăn, gồm nhiều buổi, có yếu tố political [ví dụ: tại sao lại outsource thay vì tự làm; tại sao lại outsource sang VN mà không phải là ở India, etc] liên quan đến khách hàng. Các buổi họp này có đại diện cấp cao nhất của khách hàng tham gia.</p>', 4, 3, 34, now(), now()),
        (35, 'Independently interact with clients and provide guidance and supervision to others in this area ', 'Giao tiếp độc lập với khách hàng và hướng dẫn, giám sát, trợ giúp người khác ', 4, 3, 35, now(), now()),
        (36, 'Recognized as possessing excellent communication and facilitation skills as well as negotiation skills.', 'Không chỉ excellent về communications mà còn là excellent về negotiation. Có thể thương lượng những vấn đề mà người khác không thể thương lượng nổi', 5, 3, 36, now(), now()),
        (37, 'Demonstrate effective skills in influencing Firm culture.', 'Trong quá trình làm việc ở công ty: Có kỹ năng và ảnh hưởng ở mức độ văn hóa của tổ chức', 5, 3, 37, now(), now()),
        (38, 'Demonstrate appropriate interpersonal skills required to serve as Executive Sponsor in key client relationships and/or represents Company in strategic business partnerships.', 'Là người đại diện công ty để thiết lập và duy trì mối quan hệ với những khách hàng chủ chốt, quan trọng của công ty. Đại diện công ty trong các mối quan hệ kinh doanh chiến lược. “Không có tôi thì không xong”', 5, 3, 38, now(), now()),
        (39, 'Able to improve skill knowledge to adapt yourself to new requirements', '<p>Bằng chứng [bao gồm, không chỉ gồm]:</p><p>Đơn giản nhất là hoàn tất training plan cho newcomer và được đánh giá tốt [đúng hạn, kết quả kiểm tra tốt chẳng hạn]</p><p>Tham gia các khóa huấn luyện của công ty và thi lần đầu là pass ngay</p><p>Nâng cao trình độ Anh văn đúng thời điểm như đã cam kết với công ty</p><p>Tìm hiểu vấn đề mới để giải quyết công việc trong dự án đạt đúng ước lượng hợp lý đã đề ra</p>', 1, 4, 39, now(), now()),
        (40, 'Able to write report if having any request', 'Good meeting minutes, các báo cáo về một vấn đề nào đó khi được yêu cầu', 1, 4, 40, now(), now()),
        (41, 'Estimate how much time one has, to allocate it effectively, and to stay within time limits and deadlines.', '<p>Thể hiện ở 2 khía cạnh:</p><p>Với chừng đó thời gian có được thì cần hoàn tất khối lượng công việc tương xứng ==&gt; đây là thứ cần được hỏi và trả lời hàng ngày: <strong>Do I finish a reasonable amount of tasks today? Do the peer / direct manager / customer happy with my achievement?</strong> Nếu thời gian đang được sử dụng không hiệu quả do yếu tố khách quan thì cần có phản hồi với những người có liên quan [ví dụ: suốt ngày đi họp mà lại sử dụng thời gian họp kém hiệu quả …]</p><p>Với chừng đó thời gian có được thì không over-commit để rồi không đạt được commitment</p>', 1, 4, 41, now(), now()),
        (42, 'Arrange and finish tasks creatively ', '', 1, 4, 42, now(), now()),
        (43, 'Complete assigned tasks within scheduled completion dates. Communicate potential issues as soon as they are known.', '', 1, 4, 43, now(), now()),
        (44, 'Prioritize duties in a manner consistent with project objectives / goal', '', 1, 4, 44, now(), now()),
        (45, 'Organize tasks and make an effective plan for own task', 'Hoàn tất tasks đúng hạn như ước lượng hợp lý đã đề ra', 2, 4, 45, now(), now()),
        (46, 'Complete critical tasks on time.', 'Hoàn tất critical tasks đúng hạn. ', 2, 4, 46, now(), now()),
        (47, 'Demonstrate exceptional time management and prioritization skills', '<p>Khả năng quản lý thời gian mà mình có; khả năng đặt độ ưu tiên các tasks.</p><p>Bằng chứng: Hoàn tất tốt công việc khi cùng lúc tham gia nhiều dự án / nhiều việc xen kẽ nhau</p>', 2, 4, 47, now(), now()),
        (48, 'Consistently deliver quality, on-time task assignment outcomes as a result of good planning and organizational skills.', 'Nếu có bằng chứng consistently; không có phản ví dụ trong vòng xxx tháng [cả về chất lượng lẫn thời hạn]. Nếu hoàn tất task trong 3 hours nhưng lại tốn thêm 2 hours trở lên để đi sửa ==> phản ví dụ ', 2, 4, 48, now(), now()),
        (49, 'Assistant to develop project plans and then tracks tasks, manage scope and risk, accurately reports status to client/project manager/delivery manager.', 'Hỗ trợ làm các loại project plans, kiểm soát phạm vi và rủi ro, báo cáo tình trạng dự án cho khách hàng / trưởng dự án / … Ít nhất cũng phải hỗ trợ trong khoảng 3 tháng trở lên', 2, 4, 49, now(), now()),
        (50, 'Successfully complete tasks and assignments independently and supervise the work of others as requested.', 'Hoàn tất tasks một cách độc lập [không cần hỏi người khác về cách giải quyết] và có thể kiểm tra, theo dõi, hướng dẫn, hỗ trợ người khác. Bằng chứng có khả năng là phải hoàn tất được tasks một cách độc lập trong khoảng 3 tháng trở lên; hỗ trợ được người khác ít nhất tương ứng 6 billable man-month trở lên', 2, 4, 50, now(), now()),
        (51, 'Develop project plans and then track tasks, manage scope and risk, accurately report status to client / project manager / delivery manager.', 'Ít nhất 3 tháng trở lên?', 3, 4, 51, now(), now()),
        (52, 'Develop estimates and schedules for potential follow on opportunities.', 'Follow on opportunities: Cho khách hàng tiềm năng. Ít nhất cho 3 khách hàng tiềm năng với tổng số estimated effort ít nhất khoảng 6 billable man-months', 3, 4, 52, now(), now()),
        (53, 'Successfully manage scope and client expectations to deliver Task deliverables that meet and exceed client expectations', 'Ít nhất 3 tháng trở lên? Phải có bằng chứng về việc quản lý client expectations và meet / exceed client expectations;', 3, 4, 53, now(), now()),
        (54, 'Accountable for accurate and timely feedback to project associates through Task/ Documents reviews and / or other forms of formal feedback', 'Có phản hồi chính xác và đúng hạn cho các đồng nghiệp / khách hàng hay không?', 3, 4, 54, now(), now()),
        (55, 'Successfully handle and organize strategy task', "<p><span style='text-decoration: underline;'><strong>Một số ví dụ:</strong></span></p><p><em>Handle &amp; Organize CMMI Level 3 project</em></p><p><em>Handle &amp; Organize Portfolio Management</em></p><p><em>Handle &amp; Organize: Quản lý năng suất tại công ty</em></p><p><em>Handle &amp; Organize: Quản lý nhân sự tại công ty – bài toán giữ người, bài toán lương thưởng / phúc lợi</em></p>", 4, 4, 55, now(), now()),
        (56, 'Successfully manage and organize multiple tasks at the company level', 'Successfully manage and organize multiple tasks at the company level', 4, 4, 56, now(), now()),
        (57, 'Accountable for developing his / her own management skill set to meet the company expectation. ', '<p>PMP, MBA là điều kiện cần</p><p>Cần có bằng chứng về việc áp dụng management skill set thành công</p>', 4, 4, 57, now(), now()),
        (58, 'Able to explain and use basic data structures in practical programming tasks: array, queue, stack, linkedlists, dictionary, hash, or similar.', '<p>- Hiểu được cấu trúc của những loại dữ liệu này, có thể diễn đạt cho người khác hiểu<br />- Biết được loại data nào nên được sử dụng trong trường hợp nào<br />- Đã từng sử dụng những loại dữ liệu này trong thực tế<br />→ Slot này yêu cầu developer hiểu và dùng được (mà không gặp khó khăn gì) những cấu trúc dữ liệu căn bản mà rất hay được sử dụng trong dự án thực tế</p>', 1, 5, 58, now(), now()),
        (59, 'Basic understanding of compilation and interpretation mechanisms (compiler, linker and interpreter – what are they? How they work?)', '<p>Yêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bảnYêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bản</p>', 1, 5, 59, now(), now()),
        (60, 'Basic understanding of file management, process management, memory management, cpu management in Windows and Linux & Operating System that one is working on.', '<p>Yêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bản</p>', 1, 5, 60, now(), now()),
        (61, 'Basic understanding of common concepts in programming paradigms (i.e OOP, Encapsulation, Inheritance, Design patterns, function, method, etc.)', '<p>Có thể diễn đạt được cho người khác hiểu các khái niệm dùng trong lập trình như Encapsulation, Inheritance, Polymorphism, function, method, attribute, property, design pattern<br />→ Slot này giúp dev có thể communicate với nhau bằng các thuật ngữ kỹ thuật dễ dàng hơn, giúp dev đọc hiểu các tài liệu lập trình dễ dàng</p><p>Có thể sinh viên ra trường thì mặc định biết rồi (có bằng tốt nghiệp)</p>', 1, 5, 61, now(), now()),
        (62, 'Understand Tree, Graph, simple greedy and divide and conquer algorithms.', "<p>Hiểu được cấu trúc của Tree, Graph, khác nhau cơ bản giữa Tree với Graph.<br />Hiểu được khái niệm của greedy, divide &amp; conquer algorithm.<br />Hiểu được một số giải thuật đơn giản như merge sort, quick sort, binary search, Dijkstra's Minimal Spanning Tree.</p>", 2, 5, 62, now(), now()),
        (63, 'Understand advanced operating systems concepts like kernel mode vs user mode, multi-threading, synchronization primitives and how they’re implemented.', '<p>Biết được kernel mode và user mode là gì, sự khác nhau giữa chúng, ở điều kiện nào thì ứng dụng sẽ chạy ở user mode, kernel mode.<br />Hiểu được thread và process khác nhau thế nào, multithreaded application là thế nào, biết được những vấn đề sẽ gặp phải trong multithreading programming, và cách giải quyết những vấn đề đó như thế nào<br />Biết được những cơ chế để dồng bộ hóa như mutex, semaphore.... hiểu được trong trường hợp nào n��n sử dụng cơ chế nào<br />Hiểu được cơ chế locking, khi nào cần sử dụng, sử dụng như thế nào, có sự hiểu biết cơ bản locking được hiện thực như thế nào.</p>', 2, 5, 63, now(), now()),
        (64, 'Understand common network protocols (TCP / UDP, SSH, FTP, SFTP, SMTP, HTTP, HTTPs, WS, IMAP)  and able to work with at least three of them in code.', '<p>Hiểu được sự khác nhau giữa TCP vs UDP, IMAP vs SMTP, HTTP vs HTTPS vs Websocket, SSH vs FTP vs SFTP, biết được chúng được dùng trong những trường hợp nào<br />Có bằng chứng đã làm việc với ít nhất 3 protocol ở mức độ code (sử dụng thư viện là ok)<br />→ Slot này yêu cầu developer có sự hiểu biết về những network protocol cơ bản và hay được sử dụng, dồng thời yêu cầu dev có thể ứng dụng những protocol này trong code</p>', 2, 5, 64, now(), now()),
        (65, 'Understand and able to work with common cryptography algorithms / functions like Base32/64, SHA, MD5, AES, DES, RSA, etc.', '<p>Hiểu được ứng dụng của các thuật toán mã hóa hay được sử dụng như Base32/64, SHA, MD5, DES, RSA, AES...<br />Biết được mức độ bảo mật của chúng, trong trường hợp nào nên sử dụng loại nào, có bằng chứng sử dụng những thuật toán mã hóa này trong code<br />→ Những ứng dụng cần đến bảo mật thì không thể thiếu những kiến thức căn bản như trong slot này</p>', 2, 5, 65, now(), now()),
        (66, 'Very good understanding of entire programming stack: hardware (CPU + Memory + Cache + Interrupts + microcode), binary code, assembly, static and dynamic linking, compilation, interpretation, JIT compilation, garbage collection, heap, stack, memory addressing…', '<p>Có thể giải thích cho người khác hiểu những khái niệm này<br />Có kiến thức tốt về những khái niệm này (có thể giải thích cho người khác), biết được một ứng dụng khi chuyển hóa từ code sang mã máy sẽ qua những giai đoạn nào, khi được chạy nó tương tác với các thư viện, bộ nhớ, hệ điều hành, phần cứng như thế nào<br />→ Có sự hiểu biết chi tiết về sự hoạt động / tương tác của một ứng dụng trên nền tảng hệ điều hành + phần cứng → giúp cho việc optimize application được hiệu quả</p>', 3, 5, 66, now(), now()),
        (67, 'Know resource management (file / cpu / memory / process management) in Windows and Linux & OS that one is working on in details, understand how those things affect application performance and deployment, so one is able to use those knowledge for effective problem troubleshooting, code optimization, system administration (setup, configuration, tuning, hardware selection), application deployment', '<p>Level nâng cao của 1C<br />Hiểu được sự quản lý file / memory / cpu của hệ điều hành ảnh hưởng đến ứng dụng như thế nào, biết cách hệ điều hành phân phát/giới hạn những resource này như thế nào, làm sao để gỡ bỏ những giới hạn này<br />→ Slot này yêu cầu dev có kiến thức tốt về hệ điều hành để có thể giải quyết sự cố, cài đặt, optimize ứng dụng</p>', 3, 5, 67, now(), now()),
        (68, 'Understand, able to detect and handle issues related to distributed systems (concurrency, parallel, locking, race condition, replication, transaction, etc)', 'Biết được những vấn đề sẽ gặp phải khi nhắc đến distributed system, có thể design, đưa ra giải pháp để xử lý những vấn đề đó', 3, 5, 68, now(), now()),
        (69, 'Very good understanding of machine learning and data analytics algorithms.', 'Đã từng tham gia một product [10 man-month] liên quan tới mảng này, propose được solution hoặc pass interview với khách hàng về mảng', 4, 5, 69, now(), now()),
        (70, 'Very good understanding of machine learning and data analytics algorithms.', '<p>Như slot đã miêu tả: cần có output tương ứng<br />+ Sản phảm liên quan (thư viện, implmentation trong 1 product)<br />+ Proposal liên quan (passed)</p>', 4, 5, 70, now(), now()),
        (71, 'Able to use basic features of version control system, bug tracking system like git, svn, jira, mantis etc.', 'Sử dụng tốt những chức năng cơ bản như là tạo/update thông tin task, bug một cách hiệu quả, có thể pull / push code, tạo branch, swich branch v.v...', 1, 6, 71, now(), now()),
        (72, 'Understand and follow project defined processes.', '', 1, 6, 72, now(), now()),
        (73, 'Demonstrate the ability to develop/set up simple scripts (like maven, bash scripts) to build software system.', '<p>Có bằng chứng viết script đơn giản để có thể build ứng dụng, hoặc làm những công việc có tính chất lặp lại<br />Căn bản: copy / move / delete files, setup environment variables, calling applications / utilities...</p>', 1, 6, 73, now(), now()),
        (74, 'Have written automated unit tests and come up with good unit test cases for the code that is being written.', '<p>- Tất cả các code khi viết ra phải được unit test trước khi chuyển giao cho người khác<br />- Có bằng chứng viết unit test một cách hiệu quả (test đúng cái cần test, test cover đủ những trường hợp căn bản)</p>', 1, 6, 74, now(), now()),
        (75, 'Know about some alternatives to popular and standard tools that are required by the current position.', 'Kể tên và những chức năng căn bản của một số tool (được sử dụng rộng rãi / nổi tiếng), và có thể đưa ra một vài so sánh với tool hiện tại đang dùng. Ít nhất nên có thể kể thêm 1 tool, thuộc tất cả các thể loại sau: text editor, compiler, IDE, version control system, bug tracking system.', 1, 6, 75, now(), now()),
        (76, 'Able to understand important non-functional requirements (security, concurrency & capacity, performance, reliability, maintainability, usability, documentation) and able to reflect those requirements in code / design under supervision.', 'Hiểu được ý nghĩa của những yêu cầu non-functional chính như liệt kê như trong slot, biết được thế nào thì được gọi là đạt yêu cầu / không đạt yêu cầu. Khi được hướng dẫn thì có thể hiện thực được những yêu cầu này vào design / code', 1, 6, 76, now(), now()),
        (77, 'Understand common concepts used in technologies that one is working on and understand common requirements of applications in those technology areas.', '<p>- Ví dụ nếu làm về big data thì hiểu được những concept dùng trong big data như volume, variety, velocity, veracity, stream processing, map reduce, machine learning, in-memory computing, ETL, data warehouse, data mining, data lake, cluster computing, batch processing... Hiểu được một ứng dụng về big data thông thường thì cần những yêu cầu gì<br />- Nếu làm về iOS thì hiểu được khái niệm view, controller, notification, gcd, MVC, MVVM, Core Data, protocol, location etc</p>', 1, 6, 77, now(), now()),
        (78, 'Ability to define code branching, commit rules for the project.', 'Được công nhận là người có kinh nghiệm tốt trong team về việc quản lý code → được assign, hoặc có đóng góp lớn trong việc đưa ra những yêu cầu / policy về quản lý code trong dự án', 2, 6, 78, now(), now()),
        (79, 'Very good knowledge of additional features supported by editor, debugger, linker, compiler, and use those features to improve productivity and quality of work products.', "<p>Có hiểu biết và sử dụng được những chức năng nâng cao của những tool này, từ đó mang lại hiệu quả cao hơn trong công việc / nâng cao chất lượng output, <span style='text-decoration: underline;'><strong>ví dụ:</strong></span><br /><em>- Editor / IDE: Sử dụng regular expression để search/replace text, sử dụng function list, auto code complete, code execution, debugger execution …</em><br /><em>- Debugger: debug một ứng dụng đang chạy, debug thread trong ứng dụng ….</em><br /><em>- Linker: có những flag ảnh hưởng cách thức link → ảnh hưởng đến performance</em><br /><em>- Compiler: có những flag ảnh hưởng đến chất lượng output, hoặc có những flag khi dùng sẽ cung cấp thêm những chức năng mới trong code....</em></p>", 2, 6, 79, now(), now()),
        (80, 'Have good understanding of non-functional requirements, regularly apply important non-functional requirements into design / code as a habit.', '<p>Level cao hơn của slot 1f. Có sự hiểu biết khá tốt về những yêu cầu nonfunctional, bất cứ khi nào đụng đến requirement, design, code, test thì đều mặc định có sẵn những yêu cầu nonfunctional. Ví dụ khi design GUI, output luôn luôn nhìn hợp lý (kích thước button, label, font chữ, sắp xếp các component....), khi ứng dụng chạy luôn luôn có cách để người dùng biết được nó đang chạy chứ không bị hiểu nhầm là nó đang treo…</p><p>Bằng chứng cần<br />+ Giải thích được một số protocol này<br />+ Có bằng chứng work với những protocol này ở mức thấp (chứ không phải dùng thư viện có sẵn). Ví dụ như: viết web scraping tức là đã hiểu phần nào HTTP protocol"</p>', 2, 6, 80, now(), now()),
        (81, 'Understand main software architecture patterns, their applications and their tradeoffs, have experiences with at least two of those patterns in practical projects (e.g., n-tiers, client-server pattern, SOA, microservice)', '<p>Như slot đã miêu tả: cần có output tương ứng<br />+ Design và implement được 2 hệ thống như vậy</p>', 2, 6, 81, now(), now()),
        (82, 'Work well with infrastructure/services related to technologies that one is working on.', "<p>Biết và có thể sử dụng, không cần sự trợ giúp từ người khác, nếu gặp vấn đề khó khăn có thể tự tìm cách giải quyết.<br /><em><span style='text-decoration: underline;'><strong>Ví dụ</strong></span> nếu làm về cloud computing/big data thì cần hiểu rõ và sử dụng tốt các services của Amazon web services</em></p>", 2, 6, 82, now(), now()),
        (83, 'Able to explain and use one suitable programming model/framework for technologies that one is working on', "<p><em><span style='text-decoration: underline;'><strong>Ví dụ:</strong></span> có kinh nghiệm/sử dụng thành thạo và có thể train cho team members một programming model/framework liên quan đến công nghệ mà công ty đang focus,</em></p><p><em><span style='text-decoration: underline;'><strong>Ví dụ</strong></span> Hadoop's MapReduce for Big Data/Cloud Computing, parallel prgramming model using POSIX Thread / OpenMP for C programming, OpenCL / OpenACC for HPC, Ruby on Rails</em></p>", 2, 6, 83, now(), now()),
        (84, 'Understand software system design, participate in design, implement and deployment of at least one main component (which takes around 5 man months) of a software system', 'Đọc hiểu được software design document (viết bằng UML chẳng hạn), có thể design, implement, deploy component chính của software system', 2, 6, 84, now(), now()),
        (85, 'Able to develop build automation for software systems (e.g. using scripts, Makefile, ant build, and other automation build tools), including generation of documentation, release notes and tag the code in source control.', '<p>Có bằng chứng<br />+ Product có Makefile, documentation<br />+ Thư viện publish over the internet</p>', 3, 6, 85, now(), now()),
        (86, 'Able to set up of automated functional, load/performance and UI tests in connection with CI/CD tools and virtual infrastructures.', 'Self explained', 3, 6, 86, now(), now()),
        (87, 'Very good knowledge of editors, debuggers, IDEs, open source alternatives etc. Able to evaluate/compare and give decision which one to use for project team.', 'Self explained', 3, 6, 87, now(), now()),
        (88, 'Understanding of security/safety practices and ability of proposing complete security/safety solutions.', '<p>Cần bằng chứng <br />+ Đã giải quyết vấn đề<br />+ Đã document được cái gì đó liên quan và train cho team<br />+ Pass interview liên quan ở mức độ nhất định<br />+ Có chứng chỉ về security của RedHat, IBM...</p>', 3, 6, 88, now(), now()),
        (89, 'Excellent knowledge (referred by company for guidance, can teach others) of a specific application domain that company is focusing (e.g. ecommerce, healthcare, banking, supply chain, financial services, etc)', '<p>Bằng chứng:<br />+ Có chứng chỉ liên quan<br />+ Run (as a technical lead) của một dự án với domain knowledge liên quan</p>', 3, 6, 89, now(), now()),
        (90, 'Able to evaluate & select suitable solution & design a big and complete software system which takes around 60 man months', '<p>+ Chịu trách nhiệm đưa ra giải pháp, design, deploy cho một hệ thống phần mềm lớn, ước lượng khoảng 60 man months hoặc hơn.<br />+ Pass interview của khách hàng để lấy được dự án lớn cho cty (60 man months trử lên)</p>', 3, 6, 90, now(), now()),
        (91, 'Expert in a specific application domain (enterprise, ecommerce, logistics...). Able to develop/propose key software engineering techniques for products/solutions in the domain.', '<p><strong>Expert:</strong> Không cần hỏi ai, có thể cung cấp training, hướng dẫn cho người khác, khi nhắc đến domain đó thì được công ty refer đến (training hoặc phỏng vấn với khách hàng để lấy dự án), có thể đưa ra giải pháp cho ứng dụng liên quan đến domain đó</p>', 3, 6, 91, now(), now()),
        (92, 'Select / Develop Software Development Life Cycle / Processes at company level. Perform process assessment and improvement at company level.', 'Self explained', 4, 6, 92, now(), now()),
        (93, 'Able to design systems with multiple product lines and integrate with external systems. Also should be able to design operation support systems like monitoring, reporting, fail overs etc...', 'Launch (as a technical lead) được 1 product hoặc service, ví dụ như BlueSky trong tương lai which is comprised of: web application, mobile application, micro services, cloud storage...', 4, 6, 93, now(), now()),
        (94, 'Expert in 2 or more application domains (enterprise, ecommerce, logistics...). Able to develop/propose key software engineering techniques for products / solutions in those domains.', '<p><strong>Expert:</strong> Không cần hỏi ai, có thể cung cấp training, hướng dẫn cho người khác, khi nhắc đến domain đó thì được công ty refer đến (training / phỏng vấn lấy dự án v.v....), có thể đưa ra giải pháp cho ứng dụng liên quan đến domain đó</p>', 4, 6, 94, now(), now()),
        (95, 'Knowledge of various frameworks that cover various platforms / technologies including web, desktop, server side, mobile, IoT, blockchain, big data, machine learning', '<p>Có kiến thức rất rộng về các framework (<strong>ví dụ</strong> <em>Xamarin, React Native, PhoneGap, Django, Angular JS, METEOR, .NET, Laravel, Symfony, CodeIgniter, Zend, Cocoa Touch, POSIX, Spring MVC, Struts, Hibernate, Rails, Hadoop, Storm, Samza, Spark, Flink, Hyperledger, Ethereum v.v...</em>) không yêu cầu chuyên sâu, chỉ cần ở mức khái niệm, chức năng, điểm mạnh, yếu của các framework này, có thể so sánh các framework và đưa ra quyết định nên sử dụng cái nào, để khi có requirements thì có thể nói ngay ra được nên sử dụng framework nào là tốt nhất.</p>', 4, 6, 95, now(), now()),
        (96, 'Apply clean code principles (e.g., w.r.t. names, functions, comments, format,..) as a habit.', '<p>Follow coding standard trong dự án.<br />Nếu dự án thiếu coding standard thì code khi được viết ra nên follow theo một chuẩn nào đó nổi tiếng hoặc được sử dụng rộng rãi trong cộng đồng</p>', 1, 7, 96, now(), now()),
        (97, 'Good knowledge at and apply error/exception handling as a habit.', '<p>Biết cách kiểm tra lỗi, bắt exception.<br />Biết được khi nào cần, khi nào không cần kiểm tra, bắt exception, áp dụng hàng ngày trong việc viết code như thói quen.</p>', 1, 7, 97, now(), now()),
        (98, 'Have good sense of detecting missing / conflicting specifications/requirements for programming tasks.', 'Khi đọc / tìm hiểu requirement / design, có thể phát hiện ra được những điểm bất hợp lý (cơ bản) ngay, và communicate với người khác về những vấn đề đó', 1, 7, 98, now(), now()),
        (99, 'Familiar (understand and able to use all standard data types, language features, libraries) with at least one major programming language (e.g. C/C++, VB.NET, C#, Ruby on Rails, Java, etc.)', 'Hiểu và có thể sử dụng một ngôn ngữ lập trình ở mức độ căn bản: các kiểu dữ liệu căn bản, các chức năng, thư viện của ngôn ngữ lập trình, kinh nghiệm khoảng 1 năm.', 1, 7, 99, now(), now()),
        (100, 'Ability to work with two scripting languages at a basic level, or with one at advanced level (e.g. bash/shell, Perl, Python, Ruby, VBScript, Powershell).', 'basic: basic data type, control statements (if / else, loop, switch etc..), work with files advanced: work with network, signal, use/create library, almost everything', 2, 7, 100, now(), now()),
        (101, 'Ability of designing good and normalized database schemas (Relational DB, graph DB or NoSQL), and proficiency in using and programming of views, stored procedures, triggers, user defined types, indexes.', '', 2, 7, 101, now(), now()),
        (102, "Demonstrate the ability to work well with debugger, profiling tools, review other's source code and able to detect defects and explain techniques to fix defects.", '<p>- Thường sử dụng debugger, profiling tool<br />- Thường reivew source code của người khác, hướng dẫn họ fix bug / implement features</p>', 2, 7, 102, now(), now()),
        (103, 'Work effectively with Regular Expression.', 'Có thể sử dụng regex trong hầu hết các trường hợp, có thể train cho người khác. Sử dụng regex không chỉ trong lập trình, mà còn sử dụng trong tool (search files, thay thế text...)', 2, 7, 103, now(), now()),
        (104, 'Proficiency in one and familiar with another major programming languages (e.g. C/C++, VB.NET, C#, Ruby on Rails, Java etc.)', '<p>Proficiency: hiểu và làm việc tốt với những thứ căn bản, và một số thứ nâng cao, không cần ai hướng dẫn, khi gặp vấn đề khó có thể tự tìm cách giải quyết (google chẳng hạn)<br />Có bằng chứng giúp đỡ người khác giải quyết vấn đề kỹ thuật hoặc training<br />(Đã tham gia làm dev / lead của dự án liên quan đến Ruby ít nhất 6 tháng --&gt; tính là proficient Ruby, tương tự cho các ngôn ngữ khác)</p>', 2, 7, 104, now(), now()),
        (105, 'Very good in administration, performance optimization and query/index optimization for databases (SQL and NoSQL)', '<p>- Có bằng chứng cài đặt, config, bảo mật cho databse<br />- Đã làm những việc liên quan đến optimize database performance: benchmark, analyze, performance tunning<br />- Đã viết những câu query phức tạp, tương tác với khối lượng dữ liệu lớn, nhiều nguồn dữ liệu khác nhau nhưng tốc độ chạy tốt<br />- Viết index có performance tốt</p>', 3, 7, 105, now(), now()),
        (106, 'Proficiency in two major programming languages (e.g. C/C++, VB.NET, C#, Ruby on Rails, Java, etc.)', '', 3, 7, 106, now(), now()),
        (107, 'Master at one major programming languague', '<p><strong>Master:</strong> biết tất tần tật, biết được điểm yếu, mạnh, cái gì có thể hiện thực được, cái gì không thể hiện thực được bằng ngôn ngữ lập trình đó. Biết được tất cả feature/data type từ đơn giản đến nâng cao, biết rất nhiều thư viện/open source để có thể hiện thực bất cứ một requirement nào, nếu không thì có thể tìm kiếm và áp dụng thư viện/open source một cách nhanh chóng và dễ dàng. Nắm rõ cách hoạt động &amp; sự tương tác giữa ứng dụng viết bằng ngôn ngữ lập trình đó với môi trường xung quanh như OS, hardware, compiler, linker để có thể viết code có performance cao.<br />Tóm lại ở level này, dev có thể làm tất cả mọi thứ với ngôn ngữ lập trình, và dễ dàng sử dụng những advanced techniques để viết code có performance cao, tương tác/tận dụng tốt phần cứng &amp; các resource của OS. Nếu không thể implement một thứ gì đó thì đó chỉ có thể là hạn chế của ngôn ngữ lập trình chứ không phải kiến thức / khả năng viết code của dev.</p>', 3, 7, 107, now(), now()),
        (108, 'Knowledge of various programming languages / scripts that cover various platforms / technologies including web, desktop, server side, scripting, mobile, IoT, blockchain, big data, machine learning', "Có kiến thức rất rộng về các ngôn ngữ lập trình / script (ví dụ Java, .NET, C, C++, Objective-C, Ruby, Python, R, Scala, JavaScript, PHP, ASP, Golang, Rust, Swift, Julia, Solidity, Bash, Shell, Powershell...) không yêu cầu chuyên sâu, chỉ cần ở mức khái niệm, chức năng, điểm mạnh, yếu của các ngôn ngữ lập trình này, có thể so sánh các ngôn ngữ lập trình và đưa ra quyết định nên sử dụng cái nào, để khi có requirements thì có thể nói ngay ra được nên sử dụng ngôn ngữ lập trình nào là tốt nhất.", 4, 7, 108, now(), now());
