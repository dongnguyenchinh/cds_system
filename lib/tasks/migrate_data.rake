      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(51, 'Develop project plans and then track tasks, manage scope and risk, accurately report status to client / project manager / delivery manager.', 'Ít nhất 3 tháng trở lên?', 3, 4, 51);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(52, 'Develop estimates and schedules for potential follow on opportunities.', 'Follow on opportunities: Cho khách hàng tiềm năng. Ít nhất cho 3 khách hàng tiềm năng với tổng số estimated effort ít nhất khoảng 6 billable man-months', 3, 4, 52);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(53, 'Successfully manage scope and client expectations to deliver Task deliverables  that meet and exceed client expectations', 'Ít nhất 3 tháng trở lên? Phải có bằng chứng về việc quản lý client expectations và meet / exceed client expectations;', 3, 4, 53);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(54, 'Accountable for accurate and timely feedback to project associates through Task/ Documents reviews and / or other forms of formal feedback', 'Có phản hồi chính xác và đúng hạn cho các đồng nghiệp / khách hàng hay không?', 3, 4, 54);
