      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(153, 'Organize tasks and make an effective plan for own task', 'Hoàn tất tasks đúng hạn như ước lượng hợp lý đã đề ra', 2, 21, 153);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(154, 'Complete critical tasks on time.', 'Hoàn tất critical tasks đúng hạn. ', 2, 21, 154);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(155, 'Demonstrate exceptional time management and prioritization skills', '<p>Khả năng quản lý thời gian mà mình có; khả năng đặt độ ưu tiên các tasks.</p><p>Bằng chứng: Hoàn tất tốt công việc khi cùng lúc tham gia nhiều dự án / nhiều việc xen kẽ nhau</p>', 2, 21, 155);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(156, 'Consistently deliver quality, on-time task assignment outcomes as a result of good planning and organizational skills.', 'Nếu có bằng chứng consistently; không có phản ví dụ trong vòng xxx tháng [cả về chất lượng lẫn thời hạn]. Nếu hoàn tất task trong 3 hours nhưng lại tốn thêm 2 hours trở lên để đi sửa ==> phản ví dụ ', 2, 21, 156);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(157, 'Assistant to develop project plans and then tracks tasks, manage scope and risk, accurately reports status to client / project manager / delivery manager.', 'Hỗ trợ làm các loại project plans, kiểm soát phạm vi và rủi ro, báo cáo tình trạng dự án cho khách hàng / trưởng dự án /… Ít nhất cũng phải hỗ trợ trong khoảng 3 tháng trở lên', 2, 21, 157);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(158, 'Successfully complete tasks and assignments independently and supervise the work of others as requested.', 'Hoàn tất tasks một cách độc lập [không cần hỏi người khác về cách giải quyết] và có thể kiểm tra, theo dõi, hướng dẫn, hỗ trợ người khác. Bằng chứng có khả năng là phải hoàn tất được tasks một cách độc lập trong khoảng 3 tháng trở lên; hỗ trợ được người khác ít nhất tương ứng 6 billable man-month trở lên', 2, 21, 158);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(159, 'Develop project plans and then track tasks, manage scope and risk, accurately report status to client / project manager / delivery manager.', 'Ít nhất 3 tháng trở lên?', 3, 21, 159);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(160, 'Develop estimates and schedules for potential follow on opportunities.', 'Follow on opportunities: Cho khách hàng tiềm năng. Ít nhất cho 3 khách hàng tiềm năng với tổng số estimated effort ít nhất khoảng 6 billable man-months', 3, 21, 160);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(161, 'Successfully manage scope and client expectations to deliver Task deliverables  that meet and exceed client expectations', 'Ít nhất 3 tháng trở lên? Phải có bằng chứng về việc quản lý client expectations và meet / exceed client expectations', 3, 21, 161);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(162, 'Accountable for accurate and timely feedback to project associates through Task / Documents reviews and / or other forms of formal feedback', 'Có phản hồi chính xác và đúng hạn cho các đồng nghiệp / khách hàng hay không?', 3, 21, 162);
      ---- level 3 -- end
