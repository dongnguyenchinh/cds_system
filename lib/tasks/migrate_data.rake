      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(386, 'Leads and manages development of relevant sections of project charter.', '• Completed project charter or portions thereof contain substantive content. ', 2, 30, 42);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(387, 'Performs stakeholder analysis, leads assessment and classification of stakeholders.', '<p>• Assessment addresses major requirements, main expectations, potential influences, and identification of project phase where stakeholder has most interest <br />• One or more classification models are used for stakeholder analysis (e.g., power / interest grid, power / influence grid, influence / impact grid, salience mode).</p>', 2, 30, 43);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(388, 'Performs tailoring the project defined processes (PDP) to meet project's objectives and qualify BESTARION's standards.', '• Obtain approval from QM', 2, 30, 44);
      ---- level 2 -- end
      ---- level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(389, 'Develops relationships to facilitate the approval processes. Resolves any authority conflicts, using project charter as basis, related to assignment of resources to the project.', '<p>• Obtain approval to proceed<br />• Charter signed by senior management, and/or sponsor in accordance with organizational expectations and complexity of the project.</p>', 3, 30, 45);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(390, 'Escalates and overcomes challenges with key stakeholders in the initial phase of a difficult project in term of constraints so that it could be completed successfully.', '• Evidence of issue logs demonstrating escalation to stakeholders as appropriate.', 3, 30, 46);
