      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(383, 'Works with expert judgment to develop the relevant sections of project charter.', '<p>• Completed or in-progress the relevant sections of the project charter <br />• Charter includes all or some of the sections in accordance with organizational expectations and complexity of the project.</p>', 1, 30, 39);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(384, 'Creates comprehensive list of stakeholders identified.', '<p>• Leads identification of appropriate focus for each stakeholder or group of stakeholders <br />• Ensures stakeholder register is consulted and updated on a regular basis to address changes in stakeholders through the life cycle of the project.</p>', 1, 30, 40);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(385, 'Ability to use expert judgment to assess feasibility of the project defined process based on constraints.', '• Suggested PDP in accordance with requirement and complexity of the project.', 1, 30, 41);
      ---- level 1 -- end
