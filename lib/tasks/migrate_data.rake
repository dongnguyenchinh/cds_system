      ----level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(58, 'Able to explain and use basic data structures in practical programming tasks: array, queue, stack, linkedlists, dictionary, hash, or similar.', '<p>- Hiểu được cấu trúc của những loại dữ liệu này, có thể diễn đạt cho người khác hiểu<br />- Biết được loại data nào nên được sử dụng trong trường hợp nào<br />- Đã từng sử dụng những loại dữ liệu này trong thực tế<br />→ Slot này yêu cầu developer hiểu và dùng được (mà không gặp khó khăn gì) những cấu trúc dữ liệu căn bản mà rất hay được sử dụng trong dự án thực tế</p>', 1, 5, 58);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(59, 'Basic understanding of compilation and interpretation mechanisms (compiler, linker and interpreter – what are they? How they work?)', '<p>Yêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bảnYêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bản</p>', 1, 5, 59);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(60, 'Basic understanding of file management, process management, memory management, cpu management in Windows and Linux & Operating System that one is working on.', '<p>Yêu cầu cả trên Windows, Linux, và OS mà dev đang phát triển (như iOS, MacOS ....) phải biết những thứ sau:<br />- Biết được trong hệ điều hành có những file/folder nào, mục đích chính của chúng dùng để làm gì → để khi cần lấy file, sửa đổi file thì có thể biết được ngay phải lấy ở đâu, có ảnh hưởng đến hệ thống không<br />- Biết được file / folder có những thuộc tính nào, cách thay đổi thuộc tính file/folder như thế nào → mức độ căn bản nhất của security<br />- Có chút khái niệm về FAT, NTFS, Ext → hiểu sơ được file trong ổ cứng được quản lý thế nào<br />- Biết được các trạng thái, vòng đời, cách xem thông tin process trong hệ thống, cách terminate process<br />- Biết sơ qua các loại memory như main memory, swap, page, cache memory, biết cách tìm thông tin memory trong hệ thống như thế nào<br />- Có kiến thức cơ bản về CPU, như tốc độ CPU là gì, cache là gì, tìm những thông tin này ở đâu trong hệ điều hành, hệ điều hành phân phát CPU cho các process như thế nào (mức độ căn bản)<br />→ Slot này giúp dev hiểu được sự tương tác giữa ứng dụng và hệ diều hành ở mức độ căn bản</p>', 1, 5, 60);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(61, 'Basic understanding of common concepts in programming paradigms (i.e OOP, Encapsulation, Inheritance, Design patterns, function, method, etc.)', '<p>Có thể diễn đạt được cho người khác hiểu các khái niệm dùng trong lập trình như Encapsulation, Inheritance, Polymorphism, function, method, attribute, property, design pattern<br />→ Slot này giúp dev có thể communicate với nhau bằng các thuật ngữ kỹ thuật dễ dàng hơn, giúp dev đọc hiểu các tài liệu lập trình dễ dàng</p><p>Có thể sinh viên ra trường thì mặc định biết rồi (có bằng tốt nghiệp)</p>', 1, 5, 61);
      ----level 1 -- end
      ----level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(62, 'Understand Tree, Graph, simple greedy and divide and conquer algorithms.', "<p>Hiểu được cấu trúc của Tree, Graph, khác nhau cơ bản giữa Tree với Graph.<br />Hiểu được khái niệm của greedy, divide &amp; conquer algorithm.<br />Hiểu được một số giải thuật đơn giản như merge sort, quick sort, binary search, Dijkstra's Minimal Spanning Tree.</p>", 2, 5, 62);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(63, 'Understand advanced operating systems concepts like kernel mode vs user mode, multi-threading, synchronization primitives and how they’re implemented.', '<p>Biết được kernel mode và user mode là gì, sự khác nhau giữa chúng, ở điều kiện nào thì ứng dụng sẽ chạy ở user mode, kernel mode.<br />Hiểu được thread và process khác nhau thế nào, multithreaded application là thế nào, biết được những vấn đề sẽ gặp phải trong multithreading programming, và cách giải quyết những vấn đề đó như thế nào<br />Biết được những cơ chế để dồng bộ hóa như mutex, semaphore.... hiểu được trong trường hợp nào nên sử dụng cơ chế nào<br />Hiểu được cơ chế locking, khi nào cần sử dụng, sử dụng như thế nào, có sự hiểu biết cơ bản locking được hiện thực như thế nào.</p>', 2, 5, 63);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(64, 'Understand common network protocols (TCP / UDP, SSH, FTP, SFTP, SMTP, HTTP, HTTPs, WS, IMAP)  and able to work with at least three of them in code.', '<p>Hiểu được sự khác nhau giữa TCP vs UDP, IMAP vs SMTP, HTTP vs HTTPS vs Websocket, SSH vs FTP vs SFTP, biết được chúng được dùng trong những trường hợp nào<br />Có bằng chứng đã làm việc với ít nhất 3 protocol ở mức độ code (sử dụng thư viện là ok)<br />→ Slot này yêu cầu developer có sự hiểu biết về những network protocol cơ bản và hay được sử dụng, dồng thời yêu cầu dev có thể ứng dụng những protocol này trong code</p>', 2, 5, 64);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(65, 'Understand and able to work with common cryptography algorithms / functions like Base32/64, SHA, MD5, AES, DES, RSA, etc.', '<p>Hiểu được ứng dụng của các thuật toán mã hóa hay được sử dụng như Base32/64, SHA, MD5, DES, RSA, AES...<br />Biết được mức độ bảo mật của chúng, trong trường hợp nào nên sử dụng loại nào, có bằng chứng sử dụng những thuật toán mã hóa này trong code<br />→ Những ứng dụng cần đến bảo mật thì không thể thiếu những kiến thức căn bản như trong slot này</p>', 2, 5, 65);
      ----level 2 -- end
      ----level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(66, 'Very good understanding of entire programming stack: hardware (CPU + Memory + Cache + Interrupts + microcode), binary code, assembly, static and dynamic linking, compilation, interpretation, JIT compilation, garbage collection, heap, stack, memory addressing…', '<p>Có thể giải thích cho người khác hiểu những khái niệm này<br />Có kiến thức tốt về những khái niệm này (có thể giải thích cho người khác), biết được một ứng dụng khi chuyển hóa từ code sang mã máy sẽ qua những giai đoạn nào, khi được chạy nó tương tác với các thư viện, bộ nhớ, hệ điều hành, phần cứng như thế nào<br />→ Có sự hiểu biết chi tiết về sự hoạt động / tương tác của một ứng dụng trên nền tảng hệ điều hành + phần cứng → giúp cho việc optimize application được hiệu quả</p>', 3, 5, 66);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(67, 'Know resource management (file / cpu / memory / process management) in Windows and Linux & OS that one is working on in details, understand how those things affect application performance and deployment, so one is able to use those knowledge for effective problem troubleshooting, code optimization, system administration (setup, configuration, tuning, hardware selection), application deployment', '<p>Level nâng cao của 1C<br />Hiểu được sự quản lý file / memory / cpu của hệ điều hành ảnh hưởng đến ứng dụng như thế nào, biết cách hệ điều hành phân phát/giới hạn những resource này như thế nào, làm sao để gỡ bỏ những giới hạn này<br />→ Slot này yêu cầu dev có kiến thức tốt về hệ điều hành để có thể giải quyết sự cố, cài đặt, optimize ứng dụng</p>', 3, 5, 67);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(68, 'Understand, able to detect and handle issues related to distributed systems (concurrency, parallel, locking, race condition, replication, transaction, etc)', 'Biết được những vấn đề sẽ gặp phải khi nhắc đến distributed system, có thể design, đưa ra giải pháp để xử lý những vấn đề đó', 3, 5, 68);
      ----level 3 -- end
      ----level 4 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(69, 'Very good understanding of machine learning and data analytics algorithms.', 'Đã từng tham gia một product [10 man-month] liên quan tới mảng này, propose được solution hoặc pass interview với khách hàng về mảng', 4, 5, 69);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(70, 'Very good understanding of machine learning and data analytics algorithms.', '<p>Như slot đã miêu tả: cần có output tương ứng<br />+ Sản phảm liên quan (thư viện, implmentation trong 1 product)<br />+ Proposal liên quan (passed)</p>', 4, 5, 70);
      ----level 4 -- end
      --competency Computer Science -- end
      --competency Software Engineering -- start
      ----level 1 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(71, 'Able to use basic features of version control system, bug tracking system like git, svn, jira, mantis etc.', 'Sử dụng tốt những chức năng cơ bản như là tạo/update thông tin task, bug một cách hiệu quả, có thể pull / push code, tạo branch, swich branch v.v...', 1, 6, 71);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(72, 'Understand and follow project defined processes.', '', 1, 6, 72);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(73, 'Demonstrate the ability to develop/set up simple scripts (like maven, bash scripts) to build software system.', '<p>Có bằng chứng viết script đơn giản để có thể build ứng dụng, hoặc làm những công việc có tính chất lặp lại<br />Căn bản: copy / move / delete files, setup environment variables, calling applications / utilities...</p>', 1, 6, 73);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(74, 'Have written automated unit tests and come up with good unit test cases for the code that is being written.', '<p>- Tất cả các code khi viết ra phải được unit test trước khi chuyển giao cho người khác<br />- Có bằng chứng viết unit test một cách hiệu quả (test đúng cái cần test, test cover đủ những trường hợp căn bản)</p>', 1, 6, 74);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(75, 'Know about some alternatives to popular and standard tools that are required by the current position.', 'Kể tên và những chức năng căn bản của một số tool (được sử dụng rộng rãi / nổi tiếng), và có thể đưa ra một vài so sánh với tool hiện tại đang dùng. Ít nhất nên có thể kể thêm 1 tool, thuộc tất cả các thể loại sau: text editor, compiler, IDE, version control system, bug tracking system.', 1, 6, 75);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(76, 'Able to understand important non-functional requirements (security, concurrency & capacity, performance, reliability, maintainability, usability, documentation) and able to reflect those requirements in code / design under supervision.', 'Hiểu được ý nghĩa của những yêu cầu non-functional chính như liệt kê như trong slot, biết được thế nào thì được gọi là đạt yêu cầu / không đạt yêu cầu. Khi được hướng dẫn thì có thể hiện thực được những yêu cầu này vào design / code', 1, 6, 76);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(77, 'Understand common concepts used in technologies that one is working on and understand common requirements of applications in those technology areas.', '<p>- Ví dụ nếu làm về big data thì hiểu được những concept dùng trong big data như volume, variety, velocity, veracity, stream processing, map reduce, machine learning, in-memory computing, ETL, data warehouse, data mining, data lake, cluster computing, batch processing... Hiểu được một ứng dụng về big data thông thường thì cần những yêu cầu gì<br />- Nếu làm về iOS thì hiểu được khái niệm view, controller, notification, gcd, MVC, MVVM, Core Data, protocol, location etc</p>', 1, 6, 77);
      ----level 1 -- end
      ----level 2 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(78, 'Ability to define code branching, commit rules for the project.', 'Được công nhận là người có kinh nghiệm tốt trong team về việc quản lý code → được assign, hoặc có đóng góp lớn trong việc đưa ra những yêu cầu / policy về quản lý code trong dự án', 2, 6, 78);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(79, 'Very good knowledge of additional features supported by editor, debugger, linker, compiler, and use those features to improve productivity and quality of work products.', '<p>Có hiểu biết và sử dụng được những chức năng nâng cao của những tool này, từ đó mang lại hiệu quả cao hơn trong công việc / nâng cao chất lượng output, <span style="text-decoration: underline;"><strong>ví dụ:</strong></span><br /><em>- Editor / IDE: Sử dụng regular expression để search/replace text, sử dụng function list, auto code complete, code execution, debugger execution …</em><br /><em>- Debugger: debug một ứng dụng đang chạy, debug thread trong ứng dụng ….</em><br /><em>- Linker: có những flag ảnh hưởng cách thức link → ảnh hưởng đến performance</em><br /><em>- Compiler: có những flag ảnh hưởng đến chất lượng output, hoặc có những flag khi dùng sẽ cung cấp thêm những chức năng mới trong code....</em></p>', 2, 6, 79);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(80, 'Have good understanding of non-functional requirements, regularly apply important non-functional requirements into design / code as a habit.', '<p>Level cao hơn của slot 1f. Có sự hiểu biết khá tốt về những yêu cầu nonfunctional, bất cứ khi nào đụng đến requirement, design, code, test thì đều mặc định có sẵn những yêu cầu nonfunctional. Ví dụ khi design GUI, output luôn luôn nhìn hợp lý (kích thước button, label, font chữ, sắp xếp các component....), khi ứng dụng chạy luôn luôn có cách để người dùng biết được nó đang chạy chứ không bị hiểu nhầm là nó đang treo…</p><p>Bằng chứng cần<br />+ Giải thích được một số protocol này<br />+ Có bằng chứng work với những protocol này ở mức thấp (chứ không phải dùng thư viện có sẵn). Ví dụ như: viết web scraping tức là đã hiểu phần nào HTTP protocol"</p>', 2, 6, 80);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(81, 'Understand main software architecture patterns, their applications and their tradeoffs, have experiences with at least two of those patterns in practical projects (e.g., n-tiers, client-server pattern, SOA, microservice)', '<p>Như slot đã miêu tả: cần có output tương ứng<br />+ Design và implement được 2 hệ thống như vậy</p>', 2, 6, 81);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(82, 'Work well with infrastructure/services related to technologies that one is working on.', '<p>Biết và có thể sử dụng, không cần sự trợ giúp từ người khác, nếu gặp vấn đề khó khăn có thể tự tìm cách giải quyết.<br /><em><span style="text-decoration: underline;"><strong>Ví dụ</strong></span> nếu làm về cloud computing/big data thì cần hiểu rõ và sử dụng tốt các services của Amazon web services</em></p>', 2, 6, 82);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(83, 'Able to explain and use one suitable programming model/framework for technologies that one is working on', "<p><em><span style='text-decoration: underline;'><strong>Ví dụ:</strong></span> có kinh nghiệm/sử dụng thành thạo và có thể train cho team members một programming model/framework liên quan đến công nghệ mà công ty đang focus,</em></p><p><em><span style="text-decoration: underline;"><strong>Ví dụ</strong></span> Hadoop's MapReduce for Big Data/Cloud Computing, parallel prgramming model using POSIX Thread / OpenMP for C programming, OpenCL / OpenACC for HPC, Ruby on Rails</em></p>", 2, 6, 83);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(84, 'Understand software system design, participate in design, implement and deployment of at least one main component (which takes around 5 man months) of a software system', 'Đọc hiểu được software design document (viết bằng UML chẳng hạn), có thể design, implement, deploy component chính của software system', 2, 6, 84);
      ----level 2 -- end
      ----level 3 -- start
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(85, 'Able to develop build automation for software systems (e.g. using scripts, Makefile, ant build, and other automation build tools) , including  generation of  documentation, release notes and tag the code in source control.', '<p>Có bằng chứng<br />+ Product có Makefile, documentation<br />+ Thư viện publish over the internet</p>', 3, 6, 85);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(86, 'Able to set up of automated functional, load/performance and UI tests in connection with CI/CD tools and virtual infrastructures.', 'Self explained', 3, 6, 86);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(87, 'Very good knowledge of editors, debuggers, IDEs, open source alternatives etc. Able to evaluate/compare and give decision which one to use for project team.', 'Self explained', 3, 6, 87);
      insert into slots(id, desc, evidence, level, competency_id, slot_id) values(88, 'Understanding of security/safety practices and ability of proposing complete security/safety solutions.', '<p>Cần bằng chứng <br />+ Đã giải quyết vấn đề<br />+ Đã document được cái gì đó liên quan và train cho team<br />+ Pass interview liên quan ở mức độ nhất định<br />+ Có chứng chỉ về security của RedHat, IBM...</p>', 3, 6, 88);
